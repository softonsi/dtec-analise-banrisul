    <replace_variables>N</replace_variables>
    <quoteString>N</quoteString>
    <sql>TRUNCATE TABLE TB_MANUTENCAO_TABELAS_HIST&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO TB_MANUTENCAO_TABELAS_HIST&#xd;&#xa;&#x28; DT_APONTAMENTO, &#xd;&#xa;  CD_DOC_IDENTF_CLIE, &#xd;&#xa;  CD_TP_IDENTF_CLIE&#x29;&#xd;&#xa;SELECT DT_APONTAMENTO, CD_DOC_IDENTF_CLIE, CD_TP_IDENTF_CLIE FROM TB_APONTAMENTO_HIST A&#xd;&#xa;WHERE SYSDATE - A.DT_HISTORICO &#x3e; &#xd;&#xa;&#x28;SELECT QT_DIA_EXCLUS_AUTOM &#x7c;&#x7c; &#x27; 00&#x3a;00&#x3a;00&#x27; FROM TB_STTUS_EVENTO &#xd;&#xa; WHERE CD_STTUS_EVENTO &#x3d; A.CD_STTUS_EVENTO_ATUAL &#xd;&#xa;       AND FL_NUNCA_EXCLUS &#x3d; 0&#x29;&#x3b;  &#xd;&#xa; &#xd;&#xa;DELETE FROM TB_EVENTO_HIST&#xd;&#xa;WHERE &#x28;DT_APONTAMENTO, CD_DOC_IDENTF_CLIE, CD_TP_IDENTF_CLIE&#x29; IN &#x28;&#xd;&#xa;        SELECT AH.DT_APONTAMENTO, AH.CD_DOC_IDENTF_CLIE, AH.CD_TP_IDENTF_CLIE FROM TB_EVENTO_HIST AH&#xd;&#xa;        JOIN TB_MANUTENCAO_TABELAS_HIST MT ON MT.DT_APONTAMENTO &#x3d; AH.DT_APONTAMENTO&#xd;&#xa;        AND MT.CD_DOC_IDENTF_CLIE &#x3d; AH.CD_DOC_IDENTF_CLIE&#xd;&#xa;        AND MT.CD_TP_IDENTF_CLIE &#x3d; AH.CD_TP_IDENTF_CLIE&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;DELETE FROM TB_DETLH_APONTAMENTO_HIST&#xd;&#xa;WHERE &#x28;DT_APONTAMENTO, CD_DOC_IDENTF_CLIE, CD_TP_IDENTF_CLIE&#x29; IN &#x28;&#xd;&#xa;        SELECT AH.DT_APONTAMENTO, AH.CD_DOC_IDENTF_CLIE, AH.CD_TP_IDENTF_CLIE FROM TB_DETLH_APONTAMENTO_HIST AH&#xd;&#xa;        JOIN TB_MANUTENCAO_TABELAS_HIST MT ON MT.DT_APONTAMENTO &#x3d; AH.DT_APONTAMENTO&#xd;&#xa;        AND MT.CD_DOC_IDENTF_CLIE &#x3d; AH.CD_DOC_IDENTF_CLIE&#xd;&#xa;        AND MT.CD_TP_IDENTF_CLIE &#x3d; AH.CD_TP_IDENTF_CLIE&#x29;&#x3b;&#xd;&#xa; &#xd;&#xa;DELETE FROM TB_APONTAMENTO_HIST&#xd;&#xa;WHERE &#x28;DT_APONTAMENTO, CD_DOC_IDENTF_CLIE, CD_TP_IDENTF_CLIE&#x29; IN &#x28;&#xd;&#xa;        SELECT AH.DT_APONTAMENTO, AH.CD_DOC_IDENTF_CLIE, AH.CD_TP_IDENTF_CLIE FROM TB_APONTAMENTO_HIST AH&#xd;&#xa;        JOIN TB_MANUTENCAO_TABELAS_HIST MT ON MT.DT_APONTAMENTO &#x3d; AH.DT_APONTAMENTO&#xd;&#xa;        AND MT.CD_DOC_IDENTF_CLIE &#x3d; AH.CD_DOC_IDENTF_CLIE&#xd;&#xa;        AND MT.CD_TP_IDENTF_CLIE &#x3d; AH.CD_TP_IDENTF_CLIE&#x29;&#x3b;</sql>
