    <single_statement>N</single_statement>
    <replace_variables>N</replace_variables>
    <quoteString>N</quoteString>
    <sql>TRUNCATE TABLE TB_MANUTENCAO_TABELAS&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO TB_MANUTENCAO_TABELAS&#xd;&#xa;&#x28; DT_APONTAMENTO, &#xd;&#xa;  CD_DOC_IDENTF_CLIE, &#xd;&#xa;  CD_TP_IDENTF_CLIE&#x29;&#xd;&#xa;SELECT DT_APONTAMENTO, CD_DOC_IDENTF_CLIE, CD_TP_IDENTF_CLIE&#xd;&#xa;FROM TB_APONTAMENTO A&#xd;&#xa;WHERE SYSDATE - &#x28;SELECT MAX&#x28;DT_EVENTO&#x29; &#xd;&#xa;                 FROM TB_EVENTO &#xd;&#xa;                 WHERE DT_APONTAMENTO &#x3d; A.DT_APONTAMENTO&#xd;&#xa;                 AND CD_DOC_IDENTF_CLIE &#x3d; A.CD_DOC_IDENTF_CLIE&#xd;&#xa;                 AND CD_TP_IDENTF_CLIE &#x3d; A.CD_TP_IDENTF_CLIE&#xd;&#xa;                 AND CD_STTUS_EVENTO &#x3d; A.CD_STTUS_EVENTO_ATUAL&#x29;&#xd;&#xa;&#x3e; &#x28;SELECT QT_DIA_TRANSP_AUTOM &#x7c;&#x7c; &#x27; 00&#x3a;00&#x3a;00&#x27; FROM TB_STTUS_EVENTO WHERE CD_STTUS_EVENTO &#x3d; A.CD_STTUS_EVENTO_ATUAL&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO TB_APONTAMENTO_HIST &#xd;&#xa;&#x28; DT_APONTAMENTO,&#xd;&#xa;  CD_DOC_IDENTF_CLIE,&#xd;&#xa;  CD_TP_IDENTF_CLIE,&#xd;&#xa;  CD_STTUS_EVENTO_ATUAL,&#xd;&#xa;  VL_APONTAMENTO,&#xd;&#xa;  VL_PONTO_CORTE,&#xd;&#xa;  DT_ATUALZ_CALCULO,&#xd;&#xa;  FL_CARENCIA,&#xd;&#xa;  FL_PONTO_CORTE,&#xd;&#xa;  FL_SUSP_LD,&#xd;&#xa;  CD_USU,&#xd;&#xa;  CD_LOTE,&#xd;&#xa;  DT_HISTORICO&#x29;&#xd;&#xa;SELECT &#xd;&#xa;  A.DT_APONTAMENTO,&#xd;&#xa;  A.CD_DOC_IDENTF_CLIE,&#xd;&#xa;  A.CD_TP_IDENTF_CLIE,&#xd;&#xa;  CASE A.CD_STTUS_EVENTO_ATUAL WHEN 0 THEN 30 ELSE A.CD_STTUS_EVENTO_ATUAL END AS CD_STTUS_EVENTO_ATUAL,&#xd;&#xa;  A.VL_APONTAMENTO,&#xd;&#xa;  A.VL_PONTO_CORTE,&#xd;&#xa;  A.DT_ATUALZ_CALCULO,&#xd;&#xa;  A.FL_CARENCIA,&#xd;&#xa;  A.FL_PONTO_CORTE,&#xd;&#xa;  A.FL_SUSP_LD,&#xd;&#xa;  A.CD_USU,&#xd;&#xa;  A.CD_LOTE,&#xd;&#xa;  SYSTIMESTAMP&#xd;&#xa;FROM TB_APONTAMENTO A&#xd;&#xa;JOIN TB_MANUTENCAO_TABELAS M ON M.DT_APONTAMENTO &#x3d; A.DT_APONTAMENTO &#xd;&#xa;                            AND M.CD_DOC_IDENTF_CLIE &#x3d; A.CD_DOC_IDENTF_CLIE&#xd;&#xa;                            AND M.CD_TP_IDENTF_CLIE &#x3d; A.CD_TP_IDENTF_CLIE&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO TB_DETLH_APONTAMENTO_HIST&#xd;&#xa;&#x28; ID_DETLH_APONTAMENTO,&#xd;&#xa;  DT_APONTAMENTO,&#xd;&#xa;  CD_DOC_IDENTF_CLIE,&#xd;&#xa;  CD_TP_IDENTF_CLIE,&#xd;&#xa;  CD_CLASSE_REGRA,&#xd;&#xa;  CD_REGRA,&#xd;&#xa;  CD_VERSAO_SISTEMA,&#xd;&#xa;  CD_TRANSACAO,&#xd;&#xa;  DS_INF_ANLSE,&#xd;&#xa;  DT_DISP_REGRA,&#xd;&#xa;  CD_LOTE,&#xd;&#xa;  DT_HISTORICO&#x29;&#xd;&#xa;SELECT &#xd;&#xa;  A.ID_DETLH_APONTAMENTO,&#xd;&#xa;  A.DT_APONTAMENTO,&#xd;&#xa;  A.CD_DOC_IDENTF_CLIE,&#xd;&#xa;  A.CD_TP_IDENTF_CLIE,&#xd;&#xa;  A.CD_CLASSE_REGRA,&#xd;&#xa;  A.CD_REGRA,&#xd;&#xa;  A.CD_VERSAO_SISTEMA,&#xd;&#xa;  A.CD_TRANSACAO,&#xd;&#xa;  A.DS_INF_ANLSE,&#xd;&#xa;  A.DT_DISP_REGRA,&#xd;&#xa;  A.CD_LOTE,&#xd;&#xa;  SYSTIMESTAMP&#xd;&#xa;FROM TB_DETLH_APONTAMENTO A&#xd;&#xa;JOIN TB_MANUTENCAO_TABELAS M ON M.DT_APONTAMENTO &#x3d; A.DT_APONTAMENTO &#xd;&#xa;                            AND M.CD_DOC_IDENTF_CLIE &#x3d; A.CD_DOC_IDENTF_CLIE&#xd;&#xa;                            AND M.CD_TP_IDENTF_CLIE &#x3d; A.CD_TP_IDENTF_CLIE&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO TB_EVENTO_HIST&#xd;&#xa;&#x28; CD_EVENTO,&#xd;&#xa;  DT_APONTAMENTO,&#xd;&#xa;  CD_DOC_IDENTF_CLIE,&#xd;&#xa;  CD_TP_IDENTF_CLIE,&#xd;&#xa;  TX_MSG,&#xd;&#xa;  CD_USU,&#xd;&#xa;  CD_STTUS_EVENTO,&#xd;&#xa;  TX_ARQUIVO_COMPL,&#xd;&#xa;  NM_ARQUIVO_COMPL,&#xd;&#xa;  DT_EVENTO,&#xd;&#xa;  FL_CADSTR_DESATULZ,&#xd;&#xa;  FL_RECUSA_ATULZ_CADSTR,&#xd;&#xa;  NM_MIME_TYPE_ARQ_COMPL,&#xd;&#xa;  DT_HISTORICO&#x29;&#xd;&#xa;SELECT &#xd;&#xa;  A.CD_EVENTO,&#xd;&#xa;  A.DT_APONTAMENTO,&#xd;&#xa;  A.CD_DOC_IDENTF_CLIE,&#xd;&#xa;  A.CD_TP_IDENTF_CLIE,&#xd;&#xa;  A.TX_MSG,&#xd;&#xa;  A.CD_USU,&#xd;&#xa;  A.CD_STTUS_EVENTO,&#xd;&#xa;  A.TX_ARQUIVO_COMPL,&#xd;&#xa;  A.NM_ARQUIVO_COMPL,&#xd;&#xa;  A.DT_EVENTO,&#xd;&#xa;  A.FL_CADSTR_DESATULZ,&#xd;&#xa;  A.FL_RECUSA_ATULZ_CADSTR,&#xd;&#xa;  A.NM_MIME_TYPE_ARQ_COMPL,&#xd;&#xa;  SYSTIMESTAMP&#xd;&#xa;FROM TB_EVENTO A&#xd;&#xa;JOIN TB_MANUTENCAO_TABELAS M ON M.DT_APONTAMENTO &#x3d; A.DT_APONTAMENTO &#xd;&#xa;                            AND M.CD_DOC_IDENTF_CLIE &#x3d; A.CD_DOC_IDENTF_CLIE&#xd;&#xa;                            AND M.CD_TP_IDENTF_CLIE &#x3d; A.CD_TP_IDENTF_CLIE&#x3b;&#xd;&#xa;      &#xd;&#xa;DELETE FROM TB_EVENTO &#xd;&#xa;WHERE CD_EVENTO IN &#x28;SELECT CD_EVENTO FROM TB_EVENTO_HIST&#x29;&#x3b;&#xd;&#xa;  &#xd;&#xa;DELETE FROM TB_DETLH_APONTAMENTO &#xd;&#xa;WHERE ID_DETLH_APONTAMENTO IN &#x28;SELECT ID_DETLH_APONTAMENTO FROM TB_DETLH_APONTAMENTO_HIST&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;DELETE FROM TB_APONTAMENTO &#xd;&#xa;WHERE &#x28;DT_APONTAMENTO, CD_DOC_IDENTF_CLIE, CD_TP_IDENTF_CLIE&#x29; IN &#x28;SELECT DT_APONTAMENTO, CD_DOC_IDENTF_CLIE, CD_TP_IDENTF_CLIE FROM TB_APONTAMENTO_HIST&#x29;&#x3b;</sql>
    <set_params>N</set_params>
