    <sql>MERGE INTO TB_FATO_APONTAMENTO d USING &#xd;&#xa;&#x28;&#xd;&#xa;SELECT&#xd;&#xa;    A.DT_APONTAMENTO,&#xd;&#xa;    A.CD_DOC_IDENTF_CLIE,&#xd;&#xa;    A.CD_TP_IDENTF_CLIE,&#xd;&#xa;	A.CD_LOTE,&#xd;&#xa;    A.VL_APONTAMENTO,&#xd;&#xa;    A.VL_PONTO_CORTE,&#xd;&#xa;    A.DT_ATUALZ_CALCULO,&#xd;&#xa;    A.FL_CARENCIA,&#xd;&#xa;    A.FL_PONTO_CORTE,&#xd;&#xa;    A.FL_SUSP_LD,&#xd;&#xa;    A.DT_ULTM_PARECER,&#xd;&#xa;    C.NM_CLIE,&#xd;&#xa;    C.SG_UF_RESID,&#xd;&#xa;    C.CD_PAIS_NACION,&#xd;&#xa;    C.VL_RENDA_FAT,&#xd;&#xa;    C.FL_PPE,&#xd;&#xa;    DS.NU_DIMENSAO_SITUACAO,&#xd;&#xa;    DR.NU_DIMENSAO_RISCO,&#xd;&#xa;    DF.NU_DIMENSAO_GRP_FINANC,&#xd;&#xa;    DU.NU_DIMENSAO_USUARIO,&#xd;&#xa;    DT.NU_DIMENSAO_TEMPO&#xd;&#xa;FROM TB_APONTAMENTO A&#xd;&#xa;LEFT JOIN TB_CAD_CLIE C ON C.CD_DOC_IDENTF_CLIE &#x3d; A.CD_DOC_IDENTF_CLIE AND C.CD_TP_IDENTF_CLIE &#x3d; A.CD_TP_IDENTF_CLIE&#xd;&#xa;LEFT JOIN TB_DIMENSAO_SITUACAO DS ON DS.CD_STTUS_EVENTO &#x3d; A.CD_STTUS_EVENTO_ATUAL&#xd;&#xa;LEFT JOIN TB_DIMENSAO_TEMPO DT ON DT.DT_FAIXA_INICIO &#x3d; TO_CHAR&#x28;TRUNC&#x28;A.DT_APONTAMENTO, &#x27;HH&#x27;&#x29; &#x2b; INTERVAL &#x27;00&#x3a;00&#x27; MINUTE TO SECOND, &#x27;DD&#x2f;MM&#x2f;YY HH24&#x3a;MI&#x3a;SS&#x27;&#x29;&#xd;&#xa;LEFT JOIN TB_DIMENSAO_USUARIO DU ON DU.CD_USU &#x3d; A.CD_USU&#xd;&#xa;LEFT JOIN TB_DIMENSAO_RISCO DR ON DR.CD_RISCO &#x3d; C.CD_RISCO_ATUAL&#xd;&#xa;LEFT JOIN TB_DIMENSAO_GRP_FINANC DF ON DF.CD_GRP_FINANC &#x3d; C.CD_GRP_FINANC&#xd;&#xa;WHERE A.CD_STTUS_EVENTO_ATUAL &#x3c;&#x3e; 2&#xd;&#xa;&#x29; ru                                                 &#xd;&#xa;ON &#x28;d.DT_APONTAMENTO &#x3d; ru.DT_APONTAMENTO AND d.CD_DOC_IDENTF_CLIE &#x3d; ru.CD_DOC_IDENTF_CLIE AND d.CD_TP_IDENTF_CLIE &#x3d; ru.CD_TP_IDENTF_CLIE AND d.CD_LOTE &#x3d; ru.CD_LOTE&#x29;&#xd;&#xa;WHEN MATCHED THEN&#xd;&#xa;	UPDATE SET&#xd;&#xa;     d.VL_APONTAMENTO &#x3d; ru.VL_APONTAMENTO,&#xd;&#xa;     d.DT_ATUALZ_CALCULO &#x3d; ru.DT_ATUALZ_CALCULO,&#xd;&#xa;	 d.FL_SUSP_LD &#x3d; ru.FL_SUSP_LD,&#xd;&#xa;	 d.DT_ULTM_PARECER &#x3d; ru.DT_ULTM_PARECER,&#xd;&#xa;	 d.NU_DIMENSAO_SITUACAO &#x3d; ru.NU_DIMENSAO_SITUACAO,&#xd;&#xa;	 d.NU_DIMENSAO_USUARIO &#x3d; ru.NU_DIMENSAO_USUARIO	 &#xd;&#xa;WHEN NOT MATCHED THEN&#xd;&#xa;     INSERT&#xd;&#xa;     &#x28;d.DT_APONTAMENTO,&#xd;&#xa;	  d.CD_DOC_IDENTF_CLIE,&#xd;&#xa;	  d.CD_TP_IDENTF_CLIE,&#xd;&#xa;	  d.CD_LOTE,&#xd;&#xa;	  d.VL_APONTAMENTO,&#xd;&#xa;	  d.VL_PONTO_CORTE,&#xd;&#xa;	  d.DT_ATUALZ_CALCULO,&#xd;&#xa;	  d.FL_CARENCIA,&#xd;&#xa;	  d.FL_PONTO_CORTE,&#xd;&#xa;	  d.FL_SUSP_LD,&#xd;&#xa;	  d.DT_ULTM_PARECER,&#xd;&#xa;	  d.NM_CLIE,&#xd;&#xa;	  d.SG_UF_RESID,&#xd;&#xa;	  d.CD_PAIS_NACION,&#xd;&#xa;	  d.VL_RENDA_FAT,&#xd;&#xa;	  d.FL_PPE,&#xd;&#xa;	  d.NU_DIMENSAO_SITUACAO,&#xd;&#xa;	  d.NU_DIMENSAO_RISCO,&#xd;&#xa;	  d.NU_DIMENSAO_GRP_FINANC,&#xd;&#xa;	  d.NU_DIMENSAO_USUARIO,&#xd;&#xa;	  d.NU_DIMENSAO_TEMPO&#xd;&#xa;&#x29;&#xd;&#xa;     VALUES&#xd;&#xa;     &#x28;ru.DT_APONTAMENTO,&#xd;&#xa;      ru.CD_DOC_IDENTF_CLIE,&#xd;&#xa;      ru.CD_TP_IDENTF_CLIE,&#xd;&#xa;	  ru.CD_LOTE,&#xd;&#xa;      ru.VL_APONTAMENTO,&#xd;&#xa;      ru.VL_PONTO_CORTE,&#xd;&#xa;      ru.DT_ATUALZ_CALCULO,&#xd;&#xa;      ru.FL_CARENCIA,&#xd;&#xa;      ru.FL_PONTO_CORTE,&#xd;&#xa;      ru.FL_SUSP_LD,&#xd;&#xa;      ru.DT_ULTM_PARECER,&#xd;&#xa;      ru.NM_CLIE,&#xd;&#xa;      ru.SG_UF_RESID,&#xd;&#xa;      ru.CD_PAIS_NACION,&#xd;&#xa;      ru.VL_RENDA_FAT,&#xd;&#xa;      ru.FL_PPE,&#xd;&#xa;      ru.NU_DIMENSAO_SITUACAO,&#xd;&#xa;      ru.NU_DIMENSAO_RISCO,&#xd;&#xa;      ru.NU_DIMENSAO_GRP_FINANC,&#xd;&#xa;      ru.NU_DIMENSAO_USUARIO,&#xd;&#xa;      ru.NU_DIMENSAO_TEMPO&#xd;&#xa;&#x29;&#x3b;</sql>
