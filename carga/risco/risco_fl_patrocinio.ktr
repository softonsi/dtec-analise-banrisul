    <replace_variables>Y</replace_variables>
    <quoteString>Y</quoteString>
    <sql>MERGE INTO TB_CAD_PESSOA CP&#xd;&#xa;USING   &#x28;&#xd;&#xa;        SELECT &#xd;&#xa;        CP.CD_LOTE_RISCO,&#xd;&#xa;        CP.CD_DOC_IDENTF_CLIE,&#xd;&#xa;        CP.CD_TP_IDENTF_CLIE,&#xd;&#xa;        CASE&#xd;&#xa;            WHEN CP.CD_TP_PESSOA &#x3d; &#x27;F&#x27; THEN MAX&#x28;O.FL_PATROCINIO&#x29;&#xd;&#xa;            WHEN CP.CD_TP_PESSOA &#x3d; &#x27;J&#x27; THEN MAX&#x28;RA.FL_PATROCINIO&#x29;&#xd;&#xa;            ELSE 0&#xd;&#xa;        END AS PATROCINIO_UPDATE&#xd;&#xa;        FROM TB_CAD_PESSOA CP&#xd;&#xa;        LEFT JOIN TB_PESSOA_RENDA PR ON PR.CD_DOC_IDENTF_CLIE &#x3d; CP.CD_DOC_IDENTF_CLIE AND PR.CD_TP_IDENTF_CLIE &#x3d; CP.CD_TP_IDENTF_CLIE&#xd;&#xa;        LEFT JOIN TB_RAMO_ATIVID RA ON RA.CD_RAMO_ATIVID &#x3d; CP.CD_RAMO_ATIVID&#xd;&#xa;        LEFT JOIN TB_OCUP O ON O.CD_OCUP &#x3d; PR.CD_OCUP&#xd;&#xa;        WHERE CP.CD_LOTE_RISCO &#x3d; &#x24;&#x7b;LOTE_RISCO&#x7d;&#xd;&#xa;        GROUP BY CP.CD_LOTE_RISCO, CP.CD_DOC_IDENTF_CLIE, CP.CD_TP_IDENTF_CLIE, CP.CD_TP_PESSOA&#xd;&#xa;        &#x29; U&#xd;&#xa;ON &#x28;CP.CD_LOTE_RISCO &#x3d; U.CD_LOTE_RISCO AND CP.CD_DOC_IDENTF_CLIE &#x3d; U.CD_DOC_IDENTF_CLIE AND CP.CD_TP_IDENTF_CLIE &#x3d; U.CD_TP_IDENTF_CLIE&#x29;&#xd;&#xa;WHEN MATCHED THEN UPDATE&#xd;&#xa;    SET CP.FL_PATROCINIO &#x3d; U.PATROCINIO_UPDATE&#x3b;</sql>
    <set_params>N</set_params>
