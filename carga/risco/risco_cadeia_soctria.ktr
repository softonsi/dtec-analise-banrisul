    <sql>TRUNCATE TABLE TB_ARVORE_SOCTRIA&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO TB_ARVORE_SOCTRIA &#x28;SOCIEDADE,CD_TP_IDENTF_SOCIEDD,CD_DOC_IDENTF_SOCIO,CD_TP_IDENTF_SOCIO,PC_PARTC_SOCIO,CD_PAIS_SOCIO,NIVEL,CAMINHO&#x29; &#xd;&#xa;SELECT DISTINCT CONNECT_BY_ROOT &#xd;&#xa;      CD_DOC_IDENTF_SOCIEDD SOCIEDADE &#xd;&#xa;     ,CD_TP_IDENTF_SOCIEDD&#xd;&#xa;     ,CD_DOC_IDENTF_SOCIO &#xd;&#xa;     ,CD_TP_IDENTF_SOCIO &#xd;&#xa;     ,PC_PARTC_SOCIO&#xd;&#xa;     ,CD_PAIS_SOCIO&#xd;&#xa;     ,LEVEL-1 NIVEL&#xd;&#xa;     ,SYS_CONNECT_BY_PATH&#x28;CD_DOC_IDENTF_SOCIO, &#x27;&#x2f;&#x27;&#x29; CAMINHO&#xd;&#xa;FROM &#x28;SELECT &#xd;&#xa;           CD_DOC_IDENTF_SOCIEDD&#xd;&#xa;          ,CD_TP_IDENTF_SOCIEDD&#xd;&#xa;          ,CD_DOC_IDENTF_SOCIO&#xd;&#xa;          ,CD_TP_IDENTF_SOCIO&#xd;&#xa;          ,PC_PARTC_SOCIO&#xd;&#xa;          ,CD_PAIS_SOCIO&#xd;&#xa;          ,LEVEL &#xd;&#xa;      FROM TB_CADEIA_SOCTRIA &#xd;&#xa;      CONNECT BY PRIOR CD_DOC_IDENTF_SOCIO &#x3d; CD_DOC_IDENTF_SOCIEDD&#xd;&#xa;&#x29; CS&#xd;&#xa;WHERE LEVEL &#x3e;&#x3d; 0 &#xd;&#xa;CONNECT BY PRIOR CD_DOC_IDENTF_SOCIO &#x3d; CD_DOC_IDENTF_SOCIEDD&#x3b;&#xd;&#xa;&#xd;&#xa;TRUNCATE TABLE TB_BNEFC_FINAL_SOCTRIA&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO TB_BNEFC_FINAL_SOCTRIA &#x28;CD_DOC_IDENTF_SOCIEDD,CD_TP_IDENTF_SOCIEDD,CD_NIVEL, CD_DOC_IDENTF_FINAL,CD_TP_IDENTF_FINAL,PC_PARTC_FINAL,CD_PAIS_FINAL,NM_CAMINHO_ARVORE&#x29;&#xd;&#xa;SELECT DISTINCT&#xd;&#xa;    B.SOCIEDADE,&#xd;&#xa;    B.CD_TP_IDENTF_SOCIEDD,&#xd;&#xa;    B.NIVEL CD_NIVEL,&#xd;&#xa;    B.CD_DOC_IDENTF_SOCIO CD_DOC_IDENTF_FIM,&#xd;&#xa;    B.CD_TP_IDENTF_SOCIO CD_TP_IDENTF_FIM,&#xd;&#xa;    B.PC_PARTC_SOCIO PC_PARTC_FIM,&#xd;&#xa;    B.CD_PAIS_SOCIO CD_PAIS_FIM,&#xd;&#xa;    B.CAMINHO NM_CAMINHO_ARVORE&#xd;&#xa;FROM TB_ARVORE_SOCTRIA B&#xd;&#xa;WHERE NOT  EXISTS&#xd;&#xa;&#x28;SELECT 1 FROM TB_ARVORE_SOCTRIA C&#xd;&#xa; WHERE C.CAMINHO LIKE &#x27;&#x25;&#x27; &#x7c;&#x7c; B.CAMINHO &#x7c;&#x7c; &#x27;&#x25;&#x27;&#xd;&#xa; AND B.CAMINHO &#x3c;&#x3e; C.CAMINHO&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;TRUNCATE TABLE TB_ARVORE_ADM&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO TB_ARVORE_ADM &#x28;ADM,CD_TP_IDENTF_ADM,CD_DOC_IDENTF_CLIE,CD_TP_IDENTF_CLIE,CD_PAIS_ADM,NIVEL,CAMINHO&#x29; &#xd;&#xa;SELECT distinct CONNECT_BY_ROOT &#xd;&#xa;    cd_doc_identf_adm adm &#xd;&#xa;   ,cd_tp_identf_adm &#xd;&#xa;   ,cd_doc_identf_clie &#xd;&#xa;   ,cd_tp_identf_clie &#xd;&#xa;   ,cd_pais_adm&#xd;&#xa;   ,LEVEL-1 nivel&#xd;&#xa;   ,SYS_CONNECT_BY_PATH&#x28;cd_doc_identf_clie, &#x27;&#x2f;&#x27;&#x29; caminho&#xd;&#xa;FROM &#x28;SELECT cd_doc_identf_adm&#xd;&#xa;          ,cd_tp_identf_adm&#xd;&#xa;          ,cd_doc_identf_clie&#xd;&#xa;          ,cd_tp_identf_clie &#xd;&#xa;          ,cd_pais_adm&#xd;&#xa;          ,LEVEL &#xd;&#xa;       FROM tb_cadeia_administrador &#xd;&#xa;       CONNECT BY PRIOR cd_doc_identf_clie &#x3d; cd_doc_identf_adm&#xd;&#xa;&#x29; cs&#xd;&#xa;WHERE LEVEL &#x3e;&#x3d; 0 &#xd;&#xa;CONNECT BY PRIOR cd_doc_identf_clie &#x3d; cd_doc_identf_adm&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;TRUNCATE TABLE TB_BNEFC_ADM_FINAL&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO TB_BNEFC_ADM_FINAL &#x28;ADM,CD_TP_IDENTF_ADM,CD_DOC_IDENTF_CLIE,CD_TP_IDENTF_CLIE,CD_PAIS_ADM,NIVEL,CAMINHO&#x29; &#xd;&#xa;SELECT DISTINCT CONNECT_BY_ROOT &#xd;&#xa;    CD_DOC_IDENTF_ADM ADM &#xd;&#xa;   ,CD_TP_IDENTF_ADM &#xd;&#xa;   ,CD_DOC_IDENTF_CLIE &#xd;&#xa;   ,CD_TP_IDENTF_CLIE &#xd;&#xa;   ,CD_PAIS_ADM&#xd;&#xa;   ,LEVEL-1 NIVEL&#xd;&#xa;   ,SYS_CONNECT_BY_PATH&#x28;CD_DOC_IDENTF_CLIE, &#x27;&#x2f;&#x27;&#x29; CAMINHO&#xd;&#xa;FROM &#x28;SELECT &#xd;&#xa;           CD_DOC_IDENTF_ADM&#xd;&#xa;          ,CD_TP_IDENTF_ADM&#xd;&#xa;          ,CD_DOC_IDENTF_CLIE&#xd;&#xa;          ,CD_TP_IDENTF_CLIE&#xd;&#xa;          ,CD_PAIS_ADM&#xd;&#xa;          ,LEVEL &#xd;&#xa;      FROM TB_CADEIA_ADMINISTRADOR &#xd;&#xa;      CONNECT BY PRIOR CD_DOC_IDENTF_CLIE &#x3d; CD_DOC_IDENTF_ADM&#xd;&#xa;&#x29; CS&#xd;&#xa;WHERE LEVEL &#x3e;&#x3d; 0 &#xd;&#xa;CONNECT BY PRIOR CD_DOC_IDENTF_CLIE &#x3d; CD_DOC_IDENTF_ADM&#x3b;</sql>
