    <entry>
      <name>Insert TB_PERFIL_INFORMACAO</name>
      <description/>
      <type>SQL</type>
      <sql>TRUNCATE TABLE TB_PERFIL_INFORMACAO&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO TB_PERFIL_INFORMACAO&#xd;&#xa;&#x28;CD_IDENTF_PERFIL,&#xd;&#xa;CD_VARIAVEL_PRIMEIRA,&#xd;&#xa;CD_VARIAVEL_SEGUNDA,&#xd;&#xa;CD_VARIAVEL_TERCEIRA,&#xd;&#xa;CD_VARIAVEL_QUARTA,&#xd;&#xa;VL_TOTAL,&#xd;&#xa;QT_TOTAL,&#xd;&#xa;VL_MEDIO_DIARIO,&#xd;&#xa;VL_DESVIO_DIARIO,&#xd;&#xa;DT_PERFIL&#x29;&#xd;&#xa;SELECT&#xd;&#xa;pd.CD_IDENTF_PERFIL,&#xd;&#xa;CD_VARIAVEL_PRIMEIRA,&#xd;&#xa;CD_VARIAVEL_SEGUNDA,&#xd;&#xa;CD_VARIAVEL_TERCEIRA,&#xd;&#xa;CD_VARIAVEL_QUARTA,&#xd;&#xa;SUM&#x28;VL_TOTAL&#x29; VL_TOTAL,&#xd;&#xa;SUM&#x28;QT_TOTAL&#x29; QT_TOTAL,&#xd;&#xa;AVG&#x28;VL_TOTAL&#x29; VL_MEDIO_DIARIO,&#xd;&#xa;STDDEV&#x28;VL_TOTAL&#x29; VL_DESVIO_DIARIO,&#xd;&#xa;SYSDATE DT_PERFIL &#xd;&#xa;FROM tb_perfil_dia pd&#xd;&#xa;JOIN tb_variavel_perfil vp ON vp.cd_identf_perfil &#x3d; pd.cd_identf_perfil&#xd;&#xa;WHERE vp.cd_tp_perfil &#x3d; 1&#xd;&#xa;  AND vp.FL_ATIVO &#x3d; 1&#xd;&#xa;  AND TRUNC&#x28;pd.dt_perfil&#x29; &#x3e;&#x3d; &#x28;TRUNC&#x28;SYSDATE&#x29; - 1 - vp.qt_dia_recuo&#x29;&#xd;&#xa;  AND TRUNC&#x28;pd.dt_perfil&#x29; &#x3c;&#x3d; &#x28;TRUNC&#x28;SYSDATE&#x29; - 1&#x29;&#xd;&#xa;GROUP BY pd.CD_IDENTF_PERFIL, &#xd;&#xa;	 	 CD_VARIAVEL_PRIMEIRA, &#xd;&#xa;		 CD_VARIAVEL_SEGUNDA, &#xd;&#xa;		 CD_VARIAVEL_TERCEIRA, &#xd;&#xa;		 CD_VARIAVEL_QUARTA&#x3b;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>dtec_v3_oracle</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>899</xloc>
      <yloc>231</yloc>
      </entry>
    <entry>
      <name>Insert TB_PERFIL_MES_CALENDARIO</name>
      <description/>
      <type>SQL</type>
      <sql>&#x2f;&#x2a; Inserindo no controle por dt_perfil, n&#xe3;o &#xe9; necessario considerar cd_identf_perfil &#x2a;&#x2f;&#xd;&#xa;INSERT INTO TB_CONTROLE_CARGA_PERFIL &#x28;DT_PERFIL, CD_IDENTF_PERFIL, DT_INICIO_CARGA_PERFIL, CD_PROCESSO&#x29;&#xd;&#xa;SELECT &#xd;&#xa;   DATA    AS DT_PERFIL,&#xd;&#xa;   0       As Cd_Identf_Perfil,&#xd;&#xa;   SYSDATE AS DT_INICIO_CARGA_PERFIL,&#xd;&#xa;   2       AS CD_PROCESSO&#xd;&#xa;From &#x28;Select Data From &#x28;&#xd;&#xa;       Select Distinct Last_Day&#x28;Dt_Perfil&#x29; As Data       &#xd;&#xa;        FROM TB_CONTROLE_CARGA_PERFIL &#xd;&#xa;        Where Cd_Processo &#x3d; 1 And Dt_Fim_Carga_Perfil Is Not Null        &#xd;&#xa;         And  Extract&#x28;Year From Dt_Perfil&#x29; &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Month From Dt_Perfil&#x29;,2,0&#x29;  &#x3c;&#x3e; Extract&#x28;Year From Sysdate&#x29;  &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Month From Sysdate&#x29;,2,0&#x29;        &#xd;&#xa;         And Trunc&#x28;Dt_Perfil&#x29; &#x3e;&#x3d; &#xd;&#xa;            &#x28;Select to_date&#x28; &#x27;01&#x27; &#x7c;&#x7c;  Lpad&#x28;Extract&#x28;Month From &#x28;add_months&#x28;Sysdate, &#x28;Vl_Param_Real &#x2a; -1&#x29;&#x29;&#x29;&#x29;,2,0&#x29; &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Year From &#x28;add_months&#x28;Sysdate, &#x28;Vl_Param_Real &#x2a; -1&#x29;&#x29;&#x29;&#x29;,4,0&#x29;, &#x27;dd&#x2f;mm&#x2f;yyyy&#x27;&#x29;  &#xd;&#xa;            From Tb_Param_Global Where Nm_Campo_Param &#x3d; &#x27;QT_MES_LIMPEZA_PERFIL&#x27;&#x29;&#xd;&#xa;        &#x29;  A&#xd;&#xa;        WHERE DATA NOT IN &#x28;SELECT DT_PERFIL FROM TB_CONTROLE_CARGA_PERFIL WHERE CD_PROCESSO &#x3d; 2 AND DT_FIM_CARGA_PERFIL IS NOT NULL&#x29;&#xd;&#xa;        Union&#xd;&#xa;        SELECT trunc&#x28;sysdate&#x29; AS DATA from dual&#xd;&#xa;    &#x29; A &#xd;&#xa;WHERE DATA IS NOT NULL&#xd;&#xa;AND NOT EXISTS &#x28;SELECT 1 FROM TB_CONTROLE_CARGA_PERFIL WHERE DT_PERFIL &#x3d; A.DATA AND CD_PROCESSO &#x3d; 2&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a; Excluindo dados do mes calendario do mes anterior, caso ele n&#xe3;o tenha sido processado at&#xe9; o final&#x2a;&#x2f;&#xd;&#xa;Delete From Tb_Perfil_Mes_Calendario&#xd;&#xa;WHERE CD_ANO_MES IN &#x28;&#xd;&#xa;  Select EXTRACT&#x28;YEAR FROM C.DATA&#x29; &#x7c;&#x7c; LPAD&#x28;EXTRACT&#x28;MONTH FROM C.DATA&#x29;, 2, 0&#x29;&#xd;&#xa;  FROM &#xd;&#xa;    &#x28;Select Data From&#xd;&#xa;       &#x28;Select Distinct last_day&#x28;dt_perfil&#x29; As Data&#xd;&#xa;        FROM TB_CONTROLE_CARGA_PERFIL &#xd;&#xa;        Where Cd_Processo &#x3d; 1 And Dt_Fim_Carga_Perfil Is Not Null        &#xd;&#xa;         AND   Extract&#x28;Year From Dt_Perfil&#x29; &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Month From Dt_Perfil&#x29;,2,0&#x29; &#x3c;&#x3e; Extract&#x28;Year From Sysdate&#x29;  &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Month From Sysdate&#x29;,2,0&#x29;        &#xd;&#xa;         And Trunc&#x28;Dt_Perfil&#x29; &#x3e;&#x3d; &#xd;&#xa;            &#x28;Select To_Date&#x28; &#x27;01&#x27; &#x7c;&#x7c;  Lpad&#x28;Extract&#x28;Month From add_months&#x28;Sysdate, &#x28;Vl_Param_Real &#x2a; -1 &#x29;&#x29;&#x29;,2,0&#x29; &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Year From add_months&#x28;Sysdate, &#x28;Vl_Param_Real &#x2a; - 1&#x29;&#x29;&#x29;,4,0&#x29;, &#x27;dd&#x2f;mm&#x2f;yyyy&#x27;&#x29;  &#xd;&#xa;              From Tb_Param_Global Where Nm_Campo_Param &#x3d; &#x27;QT_MES_LIMPEZA_PERFIL&#x27;&#x29;            &#xd;&#xa;         &#x29;  A&#xd;&#xa;         Where Data Not In&#xd;&#xa;          &#x28;SELECT TRUNC&#x28;DT_PERFIL&#x29; FROM TB_CONTROLE_CARGA_PERFIL WHERE CD_PROCESSO &#x3d; 2 AND DT_FIM_CARGA_PERFIL IS NOT NULL&#x29;&#xd;&#xa;        Union&#xd;&#xa;        SELECT TRUNC&#x28;SYSDATE&#x29; AS DATA FROM DUAL&#xd;&#xa;	  &#x29;  C     &#xd;&#xa;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a; Calculando Mes Calendario atrasado&#x28;se houver&#x29; e atual&#x2a;&#x2f;&#xd;&#xa;INSERT INTO TB_PERFIL_MES_CALENDARIO&#xd;&#xa;&#x28;CD_ANO_MES, CD_IDENTF_PERFIL, CD_VARIAVEL_PRIMEIRA, &#xd;&#xa;CD_VARIAVEL_SEGUNDA, CD_VARIAVEL_TERCEIRA, CD_VARIAVEL_QUARTA,&#xd;&#xa;VL_TOTAL, QT_TOTAL, VL_MEDIO_DIARIO, VL_DESVIO_DIARIO, DT_PERFIL&#x29;&#xd;&#xa;SELECT&#xd;&#xa; pd.CD_ANO_MES&#xd;&#xa;,pd.CD_IDENTF_PERFIL&#xd;&#xa;,pd.CD_VARIAVEL_PRIMEIRA&#xd;&#xa;,pd.CD_VARIAVEL_SEGUNDA&#xd;&#xa;,pd.CD_VARIAVEL_TERCEIRA&#xd;&#xa;,pd.CD_VARIAVEL_QUARTA &#xd;&#xa;,SUM&#x28;pd.VL_TOTAL&#x29; VL_TOTAL&#xd;&#xa;,SUM&#x28;pd.QT_TOTAL&#x29; QT_TOTAL&#xd;&#xa;,Avg&#x28;Pd.Vl_Total&#x29; Vl_Medio_Diario&#xd;&#xa;,Stddev&#x28;Pd.Vl_Total&#x29; Vl_Desvio_Diario&#xd;&#xa;,sysdate DT_PERFIL&#xd;&#xa;FROM  &#xd;&#xa;   &#x2f;&#x2a; buscando e formatando perfil_dia que devem ser analisadas&#x2a;&#x2f;  &#xd;&#xa;    &#x28;Select Dt_Perfil,    &#xd;&#xa;       Extract&#x28;Year From p.DT_PERFIL&#x29; &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Month From p.DT_PERFIL&#x29;, 2, 0&#x29; as CD_ANO_MES,                 &#xd;&#xa;       p.CD_IDENTF_PERFIL, p.CD_VARIAVEL_PRIMEIRA, p.CD_VARIAVEL_SEGUNDA, p.CD_VARIAVEL_TERCEIRA, p.CD_VARIAVEL_QUARTA, p.QT_TOTAL, p.VL_TOTAL&#xd;&#xa;     from tb_perfil_dia p, tb_variavel_perfil vp&#xd;&#xa;     where vp.cd_identf_perfil &#x3d; p.cd_identf_perfil AND vp.cd_tp_perfil &#x3d; 2 AND vp.FL_ATIVO &#x3d; 1&#x29; pd, &#xd;&#xa;     &#xd;&#xa;     &#x2f;&#x2a;Buscando CD_ANO_MES a serem processados&#x2a;&#x2f;&#xd;&#xa;    &#x28;SELECT N.DATA AS DATA, &#xd;&#xa;        Extract&#x28;Year From N.DATA&#x29; &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Month From N.DATA&#x29;, 2, 0&#x29; As Cd_Ano_Mes             &#xd;&#xa;     FROM     &#xd;&#xa;     &#x28;SELECT DATA FROM&#xd;&#xa;       &#x28;SELECT DISTINCT Last_Day&#x28;Dt_Perfil&#x29; AS DATA&#xd;&#xa;        FROM TB_CONTROLE_CARGA_PERFIL &#xd;&#xa;        Where Cd_Processo &#x3d; 1 And Dt_Fim_Carga_Perfil Is Not Null       &#xd;&#xa;         And  Extract&#x28;Year From Dt_Perfil&#x29; &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Month From Dt_Perfil&#x29;,2,0&#x29;  &#x3c;&#x3e; Extract&#x28;Year From Sysdate&#x29;  &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Month From Sysdate&#x29;,2,0&#x29;        &#xd;&#xa;         And Trunc&#x28;Dt_Perfil&#x29; &#x3e;&#x3d; &#xd;&#xa;            &#x28;Select To_Date&#x28; &#x27;01&#x27; &#x7c;&#x7c;  Lpad&#x28;Extract&#x28;Month From &#x28;Add_Months&#x28;Sysdate, &#x28;Vl_Param_Real &#x2a; -1&#x29;&#x29;&#x29;&#x29;,2,0&#x29; &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Year From &#x28;Add_Months&#x28;Sysdate, &#x28;Vl_Param_Real &#x2a; -1&#x29;&#x29;&#x29;&#x29;,4,0&#x29;, &#x27;dd&#x2f;mm&#x2f;yyyy&#x27;&#x29;  &#xd;&#xa;            From Tb_Param_Global Where Nm_Campo_Param &#x3d; &#x27;QT_MES_LIMPEZA_PERFIL&#x27;&#x29;                        &#xd;&#xa;          &#x29; A&#xd;&#xa;          WHERE DATA NOT IN &#x28;SELECT trunc&#x28;DT_PERFIL&#x29; FROM TB_CONTROLE_CARGA_PERFIL WHERE CD_PROCESSO &#x3d; 2 AND DT_FIM_CARGA_PERFIL IS NOT NULL&#x29;&#xd;&#xa;        Union&#xd;&#xa;        SELECT trunc&#x28;sysdate&#x29; AS DATA from dual&#xd;&#xa;	  &#x29;  N&#xd;&#xa;	 &#x29; M&#xd;&#xa;Where  Pd.Cd_Ano_Mes &#x3d; M.Cd_Ano_Mes&#xd;&#xa;   And Pd.Dt_Perfil &#x3e;&#x3d; To_Date&#x28; &#x27;01&#x27; &#x7c;&#x7c;  Lpad&#x28;Extract&#x28;Month From M.Data&#x29;,2,0&#x29; &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Year From M.Data&#x29;,4,0&#x29;, &#x27;dd&#x2f;mm&#x2f;yyyy&#x27;&#x29;   &#xd;&#xa;   And Pd.Dt_Perfil &#x3c;&#x3d; Last_Day&#x28;M.DATA&#x29;   &#x2f;&#x2a;menor que o ultimo dia do mes&#x2a;&#x2f;  &#xd;&#xa;GROUP BY&#xd;&#xa;pd.CD_ANO_MES,&#xd;&#xa;pd.CD_IDENTF_PERFIL,&#xd;&#xa;pd.CD_VARIAVEL_PRIMEIRA,&#xd;&#xa;pd.CD_VARIAVEL_SEGUNDA,&#xd;&#xa;pd.CD_VARIAVEL_TERCEIRA,&#xd;&#xa;pd.CD_VARIAVEL_QUARTA&#x3b;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;Limpando mes calendario antigo, de acordo com parametro&#x2a;&#x2f;&#xd;&#xa;DELETE FROM TB_PERFIL_MES_CALENDARIO&#xd;&#xa;Where Cd_Ano_Mes &#x3c;&#xd;&#xa;&#x28;Select Extract&#x28;Year From add_months&#x28;Sysdate, &#x28;Vl_Param_Real &#x2a; -1&#x29;&#x29;&#x29; &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Month From add_months&#x28;Sysdate, &#x28;Vl_Param_Real &#x2a; -1&#x29;&#x29;&#x29;, 2, 0&#x29;&#xd;&#xa; From Tb_Param_Global Where Nm_Campo_Param &#x3d; &#x27;QT_MES_LIMPEZA_PERFIL&#x27;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;Atualizando os perfis processados&#x2a;&#x2f;&#xd;&#xa;Update Tb_Controle_Carga_Perfil &#xd;&#xa;       SET DT_FIM_CARGA_PERFIL &#x3d; SYSDATE  &#xd;&#xa;Where Dt_Perfil In &#xd;&#xa; &#x28;Select Data From&#xd;&#xa;       &#x28;SELECT DISTINCT Last_Day&#x28;Dt_Perfil&#x29; AS DATA&#xd;&#xa;        FROM TB_CONTROLE_CARGA_PERFIL &#xd;&#xa;        WHERE CD_PROCESSO &#x3d; 1 AND DT_FIM_CARGA_PERFIL IS NOT NULL        &#xd;&#xa;        &#xd;&#xa;         AND Extract&#x28;Year From Dt_Perfil&#x29; &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Month From Dt_Perfil&#x29;,2,0&#x29;  &#x3c;&#x3e; Extract&#x28;Year From Sysdate&#x29;  &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Month From Sysdate&#x29;,2,0&#x29;  &#xd;&#xa;         &#xd;&#xa;         And Trunc&#x28;Dt_Perfil&#x29; &#x3e;&#x3d; &#xd;&#xa;            &#x28;Select to_date&#x28; &#x27;01&#x27; &#x7c;&#x7c;  Lpad&#x28;Extract&#x28;Month From &#x28;add_months&#x28;Sysdate, &#x28;Vl_Param_Real &#x2a; -1&#x29;&#x29;&#x29;&#x29;,2,0&#x29; &#x7c;&#x7c; Lpad&#x28;Extract&#x28;Year From &#x28;add_months&#x28;Sysdate, &#x28;Vl_Param_Real &#x2a; -1&#x29;&#x29;&#x29;&#x29;,4,0&#x29;, &#x27;dd&#x2f;mm&#x2f;yyyy&#x27;&#x29;  &#xd;&#xa;            From Tb_Param_Global Where Nm_Campo_Param &#x3d; &#x27;QT_MES_LIMPEZA_PERFIL&#x27;&#x29;&#xd;&#xa;         &#xd;&#xa;       &#x29;  A&#xd;&#xa;       Where Data Not In &#x28;SELECT TRUNC&#x28;DT_PERFIL&#x29; FROM TB_CONTROLE_CARGA_PERFIL WHERE CD_PROCESSO &#x3d; 2 AND DT_FIM_CARGA_PERFIL IS NOT NULL&#x29;&#xd;&#xa;   Union&#xd;&#xa;   SELECT trunc&#x28;sysdate&#x29; AS DATA FROM DUAL&#xd;&#xa;  &#x29;&#xd;&#xa; AND CD_PROCESSO &#x3d; 2&#x3b;</sql>
