    <sql>SELECT &#xd;&#xa;&#x27;UPDATE TB_CONTROLE_CARGA_PERFIL SET DT_INICIO_CARGA_PERFIL &#x3d; CURRENT_TIMESTAMP WHERE CD_IDENTF_PERFIL &#x3d; &#x27;&#x7c;&#x7c; CAST&#x28;vp.cd_identf_perfil AS VARCHAR2&#x28;2&#x29;&#x29; &#x7c;&#x7c; &#x27; and DT_INICIO_CARGA_PERFIL IS NULL and DT_FIM_CARGA_PERFIL IS NULL&#x3b;&#x27; &#x7c;&#x7c; &#xd;&#xa;&#x27;&#xd;&#xa;&#xd;&#xa;MERGE INTO TB_PERFIL_DIA dia&#xd;&#xa;  USING &#x28;&#xd;&#xa;        SELECT TRUNC&#x28;DT_TRANS&#x29; as DT_PERFIL, &#x27; &#x7c;&#x7c;&#xd;&#xa;        CAST&#x28;vp.cd_identf_perfil AS VARCHAR2&#x28;2&#x29;&#x29; &#x7c;&#x7c; &#x27; as CD_IDENTF_PERFIL&#x27; &#x7c;&#x7c;&#xd;&#xa;        CASE WHEN vp.nm_variavel_primeira IS NOT NULL THEN &#x27;, &#x27; &#x7c;&#x7c;&#xd;&#xa;        CAST&#x28;vp.nm_variavel_primeira AS VARCHAR2&#x28;50&#x29;&#x29; ELSE  &#x27;, 0&#x27; end &#x7c;&#x7c; &#x27; AS CD_VARIAVEL_PRIMEIRA&#x27; &#x7c;&#x7c;&#xd;&#xa;        CASE WHEN vp.nm_variavel_segunda IS NOT NULL THEN &#x27;, &#x27; &#x7c;&#x7c;&#xd;&#xa;        CAST&#x28;vp.nm_variavel_segunda AS VARCHAR2&#x28;50&#x29;&#x29;   ELSE &#x27;, 0&#x27; end &#x7c;&#x7c; &#x27; AS CD_VARIAVEL_SEGUNDA&#x27; &#x7c;&#x7c;&#xd;&#xa;        CASE WHEN vp.nm_variavel_terceira IS NOT NULL THEN &#x27;, &#x27; &#x7c;&#x7c;&#xd;&#xa;        CAST&#x28;vp.nm_variavel_terceira AS VARCHAR2&#x28;50&#x29;&#x29; ELSE &#x27;, 0&#x27; end &#x7c;&#x7c; &#x27; AS CD_VARIAVEL_TERCEIRA&#x27; &#x7c;&#x7c;&#xd;&#xa;        CASE WHEN vp.nm_variavel_quarta IS NOT NULL THEN &#x27;, &#x27; &#x7c;&#x7c;&#xd;&#xa;        CAST&#x28;vp.nm_variavel_quarta AS VARCHAR2&#x28;50&#x29;&#x29;  ELSE &#x27;, 0&#x27; end &#x7c;&#x7c; &#x27; AS CD_VARIAVEL_QUARTA&#x27; &#x7c;&#x7c; &#x27;, &#xd;&#xa;        COUNT&#x28;cd_transacao&#x29;  as QT_TOTAL, &#xd;&#xa;        SUM &#x28;vl_oper&#x29;  as VL_TOTAL, &#xd;&#xa;        SYSDATE AS DT_CALCULO_PERFIL&#xd;&#xa;        FROM TB_TRANS T&#xd;&#xa;        WHERE CD_LOTE in &#x28;select distinct cd_lote from TB_CONTROLE_CARGA_PERFIL where DT_INICIO_CARGA_PERFIL IS NOT NULL and DT_FIM_CARGA_PERFIL IS NULL and CD_IDENTF_PERFIL &#x3d; &#x27; &#x7c;&#x7c; CAST&#x28;vp.cd_identf_perfil AS VARCHAR2&#x28;2&#x29;&#x29; &#x7c;&#x7c; &#x27;&#x29;&#x27; &#x7c;&#x7c;  &#xd;&#xa;        CASE WHEN vp.nm_variavel_primeira IS NOT NULL THEN &#x27; AND &#x27; &#x7c;&#x7c;&#xd;&#xa;        CAST&#x28;vp.nm_variavel_primeira AS VARCHAR2&#x28;50&#x29;&#x29; &#x7c;&#x7c; &#x27; IS NOT NULL &#x27; else &#x27; &#x27; end &#x7c;&#x7c;&#xd;&#xa;        CASE WHEN vp.nm_variavel_segunda IS NOT NULL THEN &#x27; AND &#x27; &#x7c;&#x7c;&#xd;&#xa;        CAST&#x28;vp.nm_variavel_segunda AS VARCHAR2&#x28;50&#x29;&#x29; &#x7c;&#x7c; &#x27; IS NOT NULL &#x27; else &#x27; &#x27; end &#x7c;&#x7c;&#xd;&#xa;        CASE WHEN vp.nm_variavel_terceira IS NOT NULL THEN &#x27; AND &#x27; &#x7c;&#x7c;&#xd;&#xa;        CAST&#x28;vp.nm_variavel_terceira AS VARCHAR2&#x28;50&#x29;&#x29; &#x7c;&#x7c; &#x27; IS NOT NULL &#x27; else &#x27; &#x27; end &#x7c;&#x7c;&#xd;&#xa;        CASE WHEN vp.nm_variavel_quarta IS NOT NULL THEN &#x27; AND &#x27; &#x7c;&#x7c;&#xd;&#xa;        CAST&#x28;vp.nm_variavel_quarta AS VARCHAR2&#x28;50&#x29;&#x29; &#x7c;&#x7c; &#x27; IS NOT NULL &#x27; else &#x27; &#x27; end &#x7c;&#x7c;&#xd;&#xa;        CASE WHEN vp.nm_Filtro_adicional IS NOT NULL THEN &#x27; AND &#x27; &#x7c;&#x7c;&#xd;&#xa;        CAST&#x28;vp.nm_Filtro_adicional AS VARCHAR2&#x28;4000&#x29;&#x29;   else &#x27; &#x27; end  &#xd;&#xa;        &#x7c;&#x7c; &#x27; GROUP BY TRUNC&#x28;DT_TRANS&#x29;, &#x27; &#x7c;&#x7c;&#xd;&#xa;        CASE WHEN vp.nm_variavel_primeira IS NOT NULL THEN         CAST&#x28;vp.nm_variavel_primeira AS VARCHAR2&#x28;50&#x29;&#x29; else &#x27; &#x27; end &#x7c;&#x7c;&#xd;&#xa;        CASE WHEN vp.nm_variavel_segunda  IS NOT NULL THEN &#x27;, &#x27; &#x7c;&#x7c; CAST&#x28;vp.nm_variavel_segunda AS VARCHAR2&#x28;50&#x29;&#x29;  else &#x27; &#x27; end &#x7c;&#x7c;&#xd;&#xa;        CASE WHEN vp.nm_variavel_terceira IS NOT NULL THEN &#x27;, &#x27; &#x7c;&#x7c; CAST&#x28;vp.nm_variavel_terceira AS VARCHAR2&#x28;50&#x29;&#x29; else &#x27; &#x27; end &#x7c;&#x7c;&#xd;&#xa;        CASE WHEN vp.nm_variavel_quarta   IS NOT NULL THEN &#x27;, &#x27; &#x7c;&#x7c; CAST&#x28;vp.nm_variavel_quarta AS VARCHAR2&#x28;50&#x29;&#x29;   else &#x27; &#x27; end &#x7c;&#x7c; &#x27;&#x29; d&#xd;&#xa;ON  &#x28;TRUNC&#x28;dia.DT_PERFIL&#x29;           &#x3d; d.DT_PERFIL&#xd;&#xa; AND dia.CD_IDENTF_PERFIL    &#x3d; d.CD_IDENTF_PERFIL&#xd;&#xa; AND TRIM&#x28;dia.cd_variavel_primeira&#x29;&#x3d; TRIM&#x28;d.CD_VARIAVEL_PRIMEIRA&#x29;&#xd;&#xa; AND TRIM&#x28;dia.cd_variavel_segunda&#x29; &#x3d; TRIM&#x28;d.CD_VARIAVEL_SEGUNDA&#x29;&#xd;&#xa; AND TRIM&#x28;dia.cd_variavel_terceira&#x29;&#x3d; TRIM&#x28;d.CD_VARIAVEL_TERCEIRA&#x29;&#xd;&#xa; AND TRIM&#x28;dia.cd_variavel_quarta&#x29;  &#x3d; TRIM&#x28;d.CD_VARIAVEL_QUARTA&#x29;&#x29;&#xd;&#xa;WHEN MATCHED THEN&#xd;&#xa;  UPDATE SET QT_TOTAL &#x3d; dia.QT_TOTAL &#x2b; d.QT_TOTAL,&#xd;&#xa;             VL_TOTAL &#x3d; dia.VL_TOTAL &#x2b; d.VL_TOTAL,&#xd;&#xa;             DT_CALCULO_PERFIL &#x3d; TRUNC&#x28;SYSDATE&#x29;&#xd;&#xa;WHEN NOT MATCHED THEN&#xd;&#xa;  INSERT &#x28;DT_PERFIL, CD_IDENTF_PERFIL, cd_variavel_primeira, cd_variavel_segunda, cd_variavel_terceira, cd_variavel_quarta, QT_TOTAL,  VL_TOTAL,  DT_CALCULO_PERFIL&#x29;              &#xd;&#xa;  VALUES &#x28;d.DT_PERFIL, d.CD_IDENTF_PERFIL, d.cd_variavel_primeira, d.cd_variavel_segunda, d.cd_variavel_terceira, d.cd_variavel_quarta, d.QT_TOTAL, d.VL_TOTAL, d.DT_CALCULO_PERFIL&#x29; &#x3b;&#x27;&#xd;&#xa;&#x7c;&#x7c; &#xd;&#xa;&#x27;&#xd;&#xa;&#xd;&#xa;UPDATE TB_CONTROLE_CARGA_PERFIL SET DT_FIM_CARGA_PERFIL &#x3d; CURRENT_TIMESTAMP WHERE CD_IDENTF_PERFIL &#x3d; &#x27;&#x7c;&#x7c; CAST&#x28;vp.cd_identf_perfil AS VARCHAR2&#x28;2&#x29;&#x29; &#x7c;&#x7c; &#x27; and DT_INICIO_CARGA_PERFIL IS not NULL and DT_FIM_CARGA_PERFIL IS NULL&#x3b;&#x27;&#xd;&#xa;AS MONTA&#xd;&#xa;FROM tb_variavel_perfil vp&#xd;&#xa;WHERE vp.FL_ATIVO &#x3d; 1</sql>
