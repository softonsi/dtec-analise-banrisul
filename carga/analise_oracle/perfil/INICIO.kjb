      <sql>TRUNCATE TABLE TB_PERFIL_INFORMACAO&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO TB_PERFIL_INFORMACAO&#xd;&#xa;&#x28;CD_IDENTF_PERFIL,&#xd;&#xa;CD_VARIAVEL_PRIMEIRA,&#xd;&#xa;CD_VARIAVEL_SEGUNDA,&#xd;&#xa;CD_VARIAVEL_TERCEIRA,&#xd;&#xa;CD_VARIAVEL_QUARTA,&#xd;&#xa;VL_TOTAL,&#xd;&#xa;QT_TOTAL,&#xd;&#xa;VL_MEDIO_DIARIO,&#xd;&#xa;VL_DESVIO_DIARIO,&#xd;&#xa;DT_PERFIL&#x29;&#xd;&#xa;SELECT&#xd;&#xa;pd.CD_IDENTF_PERFIL,&#xd;&#xa;CD_VARIAVEL_PRIMEIRA,&#xd;&#xa;CD_VARIAVEL_SEGUNDA,&#xd;&#xa;CD_VARIAVEL_TERCEIRA,&#xd;&#xa;CD_VARIAVEL_QUARTA,&#xd;&#xa;SUM&#x28;VL_TOTAL&#x29; VL_TOTAL,&#xd;&#xa;SUM&#x28;QT_TOTAL&#x29; QT_TOTAL,&#xd;&#xa;AVG&#x28;VL_TOTAL&#x29; VL_MEDIO_DIARIO,&#xd;&#xa;STDDEV&#x28;VL_TOTAL&#x29; VL_DESVIO_DIARIO,&#xd;&#xa;SYSDATE DT_PERFIL &#xd;&#xa;FROM tb_perfil_dia pd&#xd;&#xa;JOIN tb_variavel_perfil vp ON vp.cd_identf_perfil &#x3d; pd.cd_identf_perfil&#xd;&#xa;WHERE vp.cd_tp_perfil &#x3d; 1&#xd;&#xa;  AND vp.FL_ATIVO &#x3d; 1&#xd;&#xa;  AND TRUNC&#x28;pd.dt_perfil&#x29; &#x3e; &#x28;SELECT TRUNC&#x28;MAX&#x28;DT_PERFIL&#x29; - vp.qt_dia_recuo&#x29; FROM TB_PERFIL_DIA&#x29;&#xd;&#xa;GROUP BY pd.CD_IDENTF_PERFIL, &#xd;&#xa;	 	 CD_VARIAVEL_PRIMEIRA, &#xd;&#xa;		 CD_VARIAVEL_SEGUNDA, &#xd;&#xa;		 CD_VARIAVEL_TERCEIRA, &#xd;&#xa;		 CD_VARIAVEL_QUARTA&#x3b;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
