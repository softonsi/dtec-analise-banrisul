    <entry>
      <name>Insert TB_PERFIL_MES_CALENDARIO</name>
      <description/>
      <type>SQL</type>
      <sql>MERGE INTO TB_MOVTO_PERFIL M                                                &#xd;&#xa;USING &#x28;select m.qtd, a.data from &#x28;select COUNT&#x28;&#x2a;&#x29; QTD from tb_movto_perfil&#x29; m, &#x28;SELECT  MIN&#x28;DT_CALCULO_PERFIL&#x29; - 1 as data FROM TB_PERFIL_DIA&#x29; a&#x29; S&#xd;&#xa;ON &#x28;S.QTD &#x3e; 0&#x29;&#xd;&#xa;WHEN NOT MATCHED &#xd;&#xa;THEN INSERT &#x28;DT_PERFIL&#x29; values &#x28;s.data&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a; Deletando perfis que devem ser recalculados&#x2f;calculados &#x2a;&#x2f;&#xd;&#xa;delete from TB_PERFIL_MES_CALENDARIO &#xd;&#xa;where CD_ANO_MES in &#x28;&#xd;&#xa;                    Select distinct &#xd;&#xa;                    CAST&#x28;TO_CHAR&#x28;DT_PERFIL, &#x27;YYYY&#x27;&#x29; AS VARCHAR2&#x28;4&#x29;&#x29; &#x7c;&#x7c; CAST&#x28;TO_CHAR&#x28;DT_PERFIL, &#x27;MM&#x27;&#x29; AS VARCHAR2&#x28;2&#x29;&#x29; AS CD_ANO_MES&#xd;&#xa;                    from tb_perfil_dia p, TB_VARIAVEL_PERFIL vp&#xd;&#xa;                    where vp.cd_identf_perfil &#x3d; p.cd_identf_perfil &#xd;&#xa;                        AND vp.cd_tp_perfil &#x3d; 2 &#xd;&#xa;                        AND vp.FL_ATIVO &#x3d; 1 &#xd;&#xa;                        AND DT_CALCULO_PERFIL &#x3e; &#x28;SELECT DT_PERFIL FROM TB_MOVTO_PERFIL&#x29;&#xd;&#xa;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a; Calculando &#x2f; Recalculando &#x2a;&#x2f;    &#xd;&#xa;INSERT INTO TB_PERFIL_MES_CALENDARIO &#xd;&#xa;&#x28;CD_ANO_MES,CD_IDENTF_PERFIL,CD_VARIAVEL_PRIMEIRA,CD_VARIAVEL_SEGUNDA,CD_VARIAVEL_TERCEIRA,CD_VARIAVEL_QUARTA,VL_TOTAL,QT_TOTAL,VL_MEDIO_DIARIO,VL_DESVIO_DIARIO,DT_PERFIL&#x29;&#xd;&#xa;SELECT &#xd;&#xa;CAST&#x28;TO_CHAR&#x28;p.DT_PERFIL, &#x27;YYYY&#x27;&#x29; AS VARCHAR2&#x28;4&#x29;&#x29; &#x7c;&#x7c; CAST&#x28;TO_CHAR&#x28;p.DT_PERFIL, &#x27;MM&#x27;&#x29; AS VARCHAR2&#x28;2&#x29;&#x29; as CD_ANO_MES      &#xd;&#xa;,p.CD_IDENTF_PERFIL&#xd;&#xa;,p.CD_VARIAVEL_PRIMEIRA&#xd;&#xa;,p.CD_VARIAVEL_SEGUNDA&#xd;&#xa;,p.CD_VARIAVEL_TERCEIRA&#xd;&#xa;,p.CD_VARIAVEL_QUARTA &#xd;&#xa;,SUM&#x28;p.VL_TOTAL&#x29; VL_TOTAL&#xd;&#xa;,SUM&#x28;p.QT_TOTAL&#x29; QT_TOTAL&#xd;&#xa;,AVG&#x28;p.VL_TOTAL&#x29; VL_MEDIO_DIARIO&#xd;&#xa;,STDDEV&#x28;p.VL_TOTAL&#x29; VL_DESVIO_DIARIO&#xd;&#xa;,SYSDATE DT_PERFIL&#xd;&#xa;FROM tb_perfil_dia p, &#xd;&#xa;  tb_variavel_perfil vp,&#xd;&#xa;  &#x28;Select distinct CAST&#x28;TO_CHAR&#x28;DT_PERFIL, &#x27;YYYY&#x27;&#x29; AS VARCHAR2&#x28;4&#x29;&#x29; &#x7c;&#x7c; CAST&#x28;TO_CHAR&#x28;DT_PERFIL, &#x27;MM&#x27;&#x29; AS VARCHAR2&#x28;2&#x29;&#x29; as CD_ANO_MES&#xd;&#xa;   From Tb_Perfil_Dia P, Tb_Variavel_Perfil Vp&#xd;&#xa;   Where Vp.Cd_Identf_Perfil &#x3d; P.Cd_Identf_Perfil &#xd;&#xa;     And Vp.Cd_Tp_Perfil &#x3d; 2 &#xd;&#xa;     And Vp.Fl_Ativo &#x3d; 1 &#xd;&#xa;     AND DT_CALCULO_PERFIL &#x3e; &#x28;SELECT DT_PERFIL FROM TB_MOVTO_PERFIL&#x29;&#x29; d&#xd;&#xa;Where vp.cd_identf_perfil &#x3d; p.cd_identf_perfil &#xd;&#xa;  AND vp.cd_tp_perfil &#x3d; 2 &#xd;&#xa;  AND vp.FL_ATIVO &#x3d; 1&#xd;&#xa;  AND d.CD_ANO_MES &#x3d; CAST&#x28;TO_CHAR&#x28;p.DT_PERFIL, &#x27;YYYY&#x27;&#x29; AS VARCHAR2&#x28;4&#x29;&#x29; &#x7c;&#x7c; CAST&#x28;TO_CHAR&#x28;p.DT_PERFIL, &#x27;MM&#x27;&#x29; AS VARCHAR2&#x28;2&#x29;&#x29;&#xd;&#xa;GROUP BY                      &#xd;&#xa;CAST&#x28;TO_CHAR&#x28;p.DT_PERFIL, &#x27;YYYY&#x27;&#x29; AS VARCHAR2&#x28;4&#x29;&#x29; &#x7c;&#x7c; CAST&#x28;TO_CHAR&#x28;p.DT_PERFIL, &#x27;MM&#x27;&#x29; AS VARCHAR2&#x28;2&#x29;&#x29;,&#xd;&#xa;p.CD_IDENTF_PERFIL,&#xd;&#xa;p.CD_VARIAVEL_PRIMEIRA,&#xd;&#xa;p.CD_VARIAVEL_SEGUNDA,&#xd;&#xa;p.CD_VARIAVEL_TERCEIRA,&#xd;&#xa;p.CD_VARIAVEL_QUARTA&#xd;&#xa;order by 1 desc&#x3b; &#xd;&#xa;&#xd;&#xa;UPDATE TB_MOVTO_PERFIL SET DT_PERFIL &#x3d; TRUNC&#x28;SYSDATE&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;Limpando mes calendario antigo, de acordo com parametro de cada perfil&#x2a;&#x2f;&#xd;&#xa;DELETE FROM TB_PERFIL_MES_CALENDARIO&#xd;&#xa;WHERE CD_ANO_MES &#x3c;&#xd;&#xa;&#x28;SELECT CAST&#x28;TO_CHAR&#x28;SYSDATE - QT_DIA_RECUO, &#x27;YYYY&#x27;&#x29; AS VARCHAR2&#x28;4&#x29;&#x29; &#x7c;&#x7c; CAST&#x28;TO_CHAR&#x28;SYSDATE - QT_DIA_RECUO, &#x27;MM&#x27;&#x29; AS VARCHAR2&#x28;2&#x29;&#x29;&#xd;&#xa;FROM TB_VARIAVEL_PERFIL WHERE CD_IDENTF_PERFIL &#x3d; TB_PERFIL_MES_CALENDARIO.CD_IDENTF_PERFIL&#x29;&#x3b;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
