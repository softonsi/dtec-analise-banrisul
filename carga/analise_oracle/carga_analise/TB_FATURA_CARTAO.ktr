    <sql>MERGE INTO TB_FATURA_CARTAO d USING &#xd;&#xa;&#x28;&#xd;&#xa;SELECT &#xd;&#xa;  FC.CD_CARTAO,&#xd;&#xa;  FC.DT_VENCTO_FATURA,&#xd;&#xa;  FC.CD_DOC_IDENTF_CLIE,&#xd;&#xa;  FC.CD_TP_IDENTF_CLIE,&#xd;&#xa;  FC.DT_FECHA_FATURA,&#xd;&#xa;  FC.DT_PAGTO,&#xd;&#xa;  FC.VL_FATURA,&#xd;&#xa;  FC.VL_COMPROMET,&#xd;&#xa;  FC.VL_SLD_CREDOR,&#xd;&#xa;  FC.CD_LOTE,&#xd;&#xa;  FC.CD_IDENTF_REF&#xd;&#xa;FROM TB_FATURA_CARTAO_RU FC&#xd;&#xa;JOIN TB_CARTAO C ON C.CD_CARTAO &#x3d; FC.CD_CARTAO&#xd;&#xa;WHERE FC.CD_LOTE &#x3d; &#x24;&#x7b;LOTE&#x7d;&#xd;&#xa;&#x29; ru &#xd;&#xa;ON &#x28;d.CD_CARTAO &#x3d; ru.CD_CARTAO AND d.DT_VENCTO_FATURA &#x3d; ru.DT_VENCTO_FATURA&#x29;&#xd;&#xa;WHEN MATCHED THEN&#xd;&#xa;  UPDATE SET &#xd;&#xa;  	 d.CD_DOC_IDENTF_CLIE &#x3d; ru.CD_DOC_IDENTF_CLIE,&#xd;&#xa;	 d.CD_TP_IDENTF_CLIE &#x3d; ru.CD_TP_IDENTF_CLIE,&#xd;&#xa;	 d.DT_FECHA_FATURA &#x3d; ru.DT_FECHA_FATURA,&#xd;&#xa;	 d.DT_PAGTO &#x3d; ru.DT_PAGTO,&#xd;&#xa;	 d.VL_FATURA &#x3d; ru.VL_FATURA,&#xd;&#xa;	 d.VL_COMPROMET &#x3d; ru.VL_COMPROMET,&#xd;&#xa;	 d.VL_SLD_CREDOR &#x3d; ru.VL_SLD_CREDOR,&#xd;&#xa;	 d.CD_LOTE &#x3d; ru.CD_LOTE,&#xd;&#xa;	 d.CD_IDENTF_REF &#x3d; ru.CD_IDENTF_REF&#xd;&#xa;  WHERE ru.CD_LOTE &#x3d; &#x24;&#x7b;LOTE&#x7d;&#xd;&#xa;WHEN NOT MATCHED THEN&#xd;&#xa;	INSERT &#xd;&#xa;	&#x28;CD_CARTAO,&#xd;&#xa;	 DT_VENCTO_FATURA,&#xd;&#xa;	 CD_DOC_IDENTF_CLIE,&#xd;&#xa;	 CD_TP_IDENTF_CLIE,&#xd;&#xa;	 DT_FECHA_FATURA,&#xd;&#xa;	 DT_PAGTO,&#xd;&#xa;	 VL_FATURA,&#xd;&#xa;	 VL_COMPROMET,&#xd;&#xa;	 VL_SLD_CREDOR,&#xd;&#xa;	 CD_LOTE,&#xd;&#xa;	 CD_IDENTF_REF&#x29;     &#xd;&#xa;    VALUES&#xd;&#xa;    &#x28;ru.CD_CARTAO,&#xd;&#xa;	 ru.DT_VENCTO_FATURA,&#xd;&#xa;	 ru.CD_DOC_IDENTF_CLIE,&#xd;&#xa;	 ru.CD_TP_IDENTF_CLIE,&#xd;&#xa;	 ru.DT_FECHA_FATURA,&#xd;&#xa;	 ru.DT_PAGTO,&#xd;&#xa;	 ru.VL_FATURA,&#xd;&#xa;	 ru.VL_COMPROMET,&#xd;&#xa;	 ru.VL_SLD_CREDOR,&#xd;&#xa;	 ru.CD_LOTE,&#xd;&#xa;	 ru.CD_IDENTF_REF&#x29;&#xd;&#xa;  WHERE ru.CD_LOTE &#x3d; &#x24;&#x7b;LOTE&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO TB_INCONS &#x28;CD_SEQ_INCONS, CD_INCONS,DS_CONTEUDO_CAMPO,TX_DETAL_OCOR,DT_OCORRENCIA,CD_LOTE,DS_SQL_ERRO&#x29;&#xd;&#xa;SELECT &#xd;&#xa;SQ_INCONS.NEXTVAL AS CD_SEQ_INCONS,&#xd;&#xa;&#x27;6&#x27; AS CD_INCONS, &#xd;&#xa;CR.CD_CARTAO &#x7c;&#x7c; &#x27;, &#x27; &#x7c;&#x7c;CR.DT_VENCTO_FATURA AS DS_CONTEUDO_CAMPO,&#xd;&#xa;&#x27;Fatura sem cart&#xe3;o&#x27; AS TX_DETAL_OCOR,&#xd;&#xa;SYSDATE AS DT_OCORRENCIA,&#xd;&#xa;CR.CD_LOTE,&#xd;&#xa;&#x27;ERRO - TB_FATURA_CARTAO - FkCartao_FaturaCartoa&#x27; AS DS_SQL_ERRO&#xd;&#xa;FROM TB_FATURA_CARTAO_RU CR&#xd;&#xa;WHERE CR.CD_LOTE &#x3d; &#x24;&#x7b;LOTE&#x7d; AND NOT EXISTS &#x28;SELECT 1 FROM TB_CARTAO WHERE CD_CARTAO &#x3d; CR.CD_CARTAO&#x29;&#x3b;</sql>
