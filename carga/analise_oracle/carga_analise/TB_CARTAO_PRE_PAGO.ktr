    <sql>MERGE INTO TB_CARTAO_PRE_PAGO D &#xd;&#xa;USING &#x28;&#xd;&#xa;SELECT &#xd;&#xa;CD_CARTAO_PRE_PAGO&#xd;&#xa;,LTRIM&#x28;RTRIM&#x28;CD_DOC_IDENTF_CLIE&#x29;&#x29; AS CD_DOC_IDENTF_CLIE&#xd;&#xa;,CD_TP_IDENTF_CLIE&#xd;&#xa;,DT_EMISS_CARTAO&#xd;&#xa;,VL_CARGA_INIC_CARTAO&#xd;&#xa;,CD_TP_IDENTF_BNEFC&#xd;&#xa;,LTRIM&#x28;RTRIM&#x28;CD_DOC_IDENTF_BNEFC&#x29;&#x29; AS CD_DOC_IDENTF_BNEFC&#xd;&#xa;,NM_BNEFC_CARTAO&#xd;&#xa;,DT_EMIS_DOC_IDENTF&#xd;&#xa;,CD_ORGAO_EXPDD&#xd;&#xa;,SG_ORG_INTERNL&#xd;&#xa;,CD_PAIS_PASSRTE_BNEFC&#xd;&#xa;,FL_PROPSTO_EMISS_CARTAO&#xd;&#xa;,SG_UF_EMISS_CARTAO&#xd;&#xa;,VL_SALDO_CARTAO&#xd;&#xa;,CD_TP_CARTAO_PRE_PAGO &#xd;&#xa;,CD_IBGE_CID_EMISS_CARTAO&#xd;&#xa;,NM_CID_EMISS_CARTAO&#xd;&#xa;,CD_LOTE&#xd;&#xa;FROM TB_CARTAO_PRE_PAGO_RU WHERE CD_LOTE &#x3d; &#x24;&#x7b;LOTE&#x7d;&#xd;&#xa;&#x29; ru                                                 &#xd;&#xa;ON &#x28;d.CD_CARTAO_PRE_PAGO &#x3d; ru.CD_CARTAO_PRE_PAGO&#x29;&#xd;&#xa;WHEN MATCHED THEN&#xd;&#xa;	UPDATE SET&#xd;&#xa;     d.CD_DOC_IDENTF_CLIE &#x3d; ru.CD_DOC_IDENTF_CLIE&#xd;&#xa;	,d.CD_TP_IDENTF_CLIE &#x3d; ru.CD_TP_IDENTF_CLIE&#xd;&#xa;	,d.DT_EMISS_CARTAO &#x3d; ru.DT_EMISS_CARTAO&#xd;&#xa;	,d.VL_CARGA_INIC_CARTAO &#x3d; ru.VL_CARGA_INIC_CARTAO&#xd;&#xa;	,d.CD_TP_IDENTF_BNEFC &#x3d; ru.CD_TP_IDENTF_BNEFC&#xd;&#xa;	,d.CD_DOC_IDENTF_BNEFC &#x3d; ru.CD_DOC_IDENTF_BNEFC&#xd;&#xa;	,d.NM_BNEFC_CARTAO &#x3d; ru.NM_BNEFC_CARTAO&#xd;&#xa;	,d.DT_EMIS_DOC_IDENTF &#x3d; ru.DT_EMIS_DOC_IDENTF&#xd;&#xa;	,d.CD_ORGAO_EXPDD &#x3d; ru.CD_ORGAO_EXPDD&#xd;&#xa;	,d.SG_ORG_INTERNL &#x3d; ru.SG_ORG_INTERNL&#xd;&#xa;	,d.CD_PAIS_PASSRTE_BNEFC &#x3d; ru.CD_PAIS_PASSRTE_BNEFC&#xd;&#xa;	,d.FL_PROPSTO_EMISS_CARTAO &#x3d; ru.FL_PROPSTO_EMISS_CARTAO&#xd;&#xa;	,d.SG_UF_EMISS_CARTAO &#x3d; ru.SG_UF_EMISS_CARTAO&#xd;&#xa;    ,d.VL_SALDO_CARTAO &#x3d; ru.VL_SALDO_CARTAO&#xd;&#xa;    ,d.CD_TP_CARTAO_PRE_PAGO &#x3d; ru.CD_TP_CARTAO_PRE_PAGO&#xd;&#xa;    ,d.CD_IBGE_CID_EMISS_CARTAO &#x3d; ru.CD_IBGE_CID_EMISS_CARTAO&#