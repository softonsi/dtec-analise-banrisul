    <quoteString>N</quoteString>
    <sql>MERGE INTO TB_PRODUTO D &#xd;&#xa;USING &#x28;SELECT CD_PRODUTO, CD_GRP_PRODUTO, NM_PRODUTO, VL_MED_INVEST, VL_MERCADO, CD_TITULO, VL_ABERT_MERCADO, VL_FECHA_MERCADO FROM TB_PRODUTO_RU&#x29; ru                                                 &#xd;&#xa;ON &#x28;d.CD_PRODUTO &#x3d; ru.CD_PRODUTO&#x29;&#xd;&#xa;WHEN MATCHED THEN&#xd;&#xa;	UPDATE SET&#xd;&#xa;    d.NM_PRODUTO &#x3d; ru.NM_PRODUTO,&#xd;&#xa;	d.VL_MED_INVEST &#x3d; ru.VL_MED_INVEST,&#xd;&#xa;	d.VL_MERCADO &#x3d; ru.VL_MERCADO,&#xd;&#xa;	d.CD_GRP_PRODUTO &#x3d; ru.CD_GRP_PRODUTO,&#xd;&#xa;	d.CD_TITULO &#x3d; ru.CD_TITULO,&#xd;&#xa;	d.VL_ABERT_MERCADO &#x3d; ru.VL_ABERT_MERCADO,&#xd;&#xa;	d.VL_FECHA_MERCADO &#x3d; ru.VL_FECHA_MERCADO    &#xd;&#xa;WHEN NOT MATCHED THEN&#xd;&#xa;     INSERT&#xd;&#xa;     &#x28;CD_PRODUTO,&#xd;&#xa;	 NM_PRODUTO,&#xd;&#xa;	 VL_MED_INVEST,&#xd;&#xa;	 VL_MERCADO,&#xd;&#xa;	 CD_GRP_PRODUTO,&#xd;&#xa;     CD_TITULO, &#xd;&#xa;     VL_ABERT_MERCADO, &#xd;&#xa;     VL_FECHA_MERCADO&#x29;     &#xd;&#xa;    VALUES&#xd;&#xa;    &#x28;ru.CD_PRODUTO,&#xd;&#xa;	 ru.NM_PRODUTO,&#xd;&#xa;	 ru.VL_MED_INVEST,&#xd;&#xa;	 ru.VL_MERCADO,&#xd;&#xa;	 ru.CD_GRP_PRODUTO,&#xd;&#xa;     ru.CD_TITULO, &#xd;&#xa;     ru.VL_ABERT_MERCADO, &#xd;&#xa;     ru.VL_FECHA_MERCADO&#x29;&#x3b;</sql>
