    <sql>Select &#xd;&#xa;&#x27;Merge Into Tb_Trans_Coaf C&#xd;&#xa;Using &#x28;Select&#xd;&#xa;  C.Dt_Ocorrencia,&#xd;&#xa;  C.Cd_Tp_Identf_Clie,&#xd;&#xa;  c.CD_DOC_IDENTF_CLIE,&#xd;&#xa;  CD_TRANSACAO,&#xd;&#xa;  CD_BANCO, &#xd;&#xa;  CD_AG,&#xd;&#xa;  CD_AG_OPER,&#xd;&#xa;  Cd_Cta,&#xd;&#xa;  Cd_Trans_Orig,&#xd;&#xa;  Cd_Natur_Oper,&#xd;&#xa;  Cd_Tp_Oper,&#xd;&#xa;  Cd_Via_Oper,&#xd;&#xa;  CD_FORMA_OPER,&#xd;&#xa;  VL_OPER,&#xd;&#xa;  DT_TRANS,&#xd;&#xa;  CD_BCO_DESTORIG, &#xd;&#xa;  CD_AG_DESTORIG, &#xd;&#xa;  CD_CTA_DESTORIG,&#xd;&#xa;  NM_DESTORIG, &#xd;&#xa;  CD_TP_IDENTF_DESTORIG,&#xd;&#xa;  Cd_Doc_Identf_Destorig,&#xd;&#xa;  NM_BNEFC_CARTAO, &#xd;&#xa;  CD_TP_IDENTF_BNEFC,&#xd;&#xa;  CD_DOC_IDENTF_BNEFC,&#xd;&#xa;  NM_PSSOA_REALZ_TRANS,&#xd;&#xa;  CD_TP_DOC_IDENTF_REALZ_TRANS,&#xd;&#xa;  Cd_Doc_Identf_Realz_Trans,&#xd;&#xa;  cd_tp_identf_movd,&#xd;&#xa;  nm_movd,&#xd;&#xa;  cd_doc_identf_movd,&#xd;&#xa;  T.Cd_Lote&#xd;&#xa;From Tb_Trans T &#xd;&#xa;  Inner Join &#x28;Select Trunc&#x28;Min&#x28;Dt_Trans&#x29;&#x29; Dt_Min, Trunc&#x28;Max&#x28;Dt_Trans&#x29;&#x29; Dt_Max From Tb_Trans Where Cd_Lote &#x3d; &#x27; &#x7c;&#x7c; &#x24;&#x7b;LOTE&#x7d; &#x7c;&#x7c; &#x27;&#x29; L&#xd;&#xa;      on trunc&#x28;dt_trans&#x29; &#x3e;&#x3d; l.dt_min and trunc&#x28;dt_trans&#x29; &#x3c;&#x3d; l.dt_max&#xd;&#xa;  Inner Join Tb_Clie_Coaf C On T.Cd_Doc_Identf_Clie &#x3d; C.Cd_Doc_Identf_Clie And T.Cd_Tp_Identf_Clie &#x3d; C.Cd_Tp_Identf_Clie And trunc&#x28;T.dt_trans&#x29; &#x3d; c.dt_ocorrencia&#xd;&#xa;   And &#x27; &#x7c;&#x7c;&#xd;&#xa;   &#x28;Select Tx_Param From Tb_Param_Global Where Nm_Campo_Param &#x3d; &#x27;pmRestricaoTrans&#x27;&#x29;   &#xd;&#xa;  &#x7c;&#x7c;&#xd;&#xa;&#x27;&#x29; T&#xd;&#xa;   On &#x28;C.Dt_Ocorrencia      &#x3d; T.Dt_Ocorrencia      And  &#xd;&#xa;       C.Cd_Doc_Identf_Clie &#x3d; T.Cd_Doc_Identf_Clie And  &#xd;&#xa;       C.Cd_Tp_Identf_Clie  &#x3d; T.Cd_Tp_Identf_Clie  And&#xd;&#xa;       C.cd_transacao       &#x3d; T.cd_transacao&#x29;  &#xd;&#xa;   When Not Matched Then  &#xd;&#xa;Insert &#x28;Dt_Ocorrencia, Cd_Tp_Identf_Clie, Cd_Doc_Identf_Clie, Cd_Transacao, Cd_Banco, Cd_Ag, CD_AG_OPER, Cd_Cta, Cd_Trans_Orig, Cd_Natur_Oper, Cd_Tp_Oper, Cd_Via_Oper,&#xd;&#xa; CD_FORMA_OPER, VL_OPER, DT_TRANS, CD_BCO_DESTORIG, CD_AG_DESTORIG, CD_CTA_DESTORIG, NM_DESTORIG, CD_TP_IDENTF_DESTORIG, Cd_Doc_Identf_Destorig,&#xd;&#xa; Nm_Bnefc_Cartao, Cd_Tp_Identf_Bnefc, Cd_Doc_Identf_Bnefc, Nm_Pssoa_Realz_Trans, Cd_Tp_Doc_Identf_Realz_Trans, Cd_Doc_Identf_Realz_Trans,&#xd;&#xa; Cd_Tp_Identf_Movd, Nm_Movd, Cd_Doc_Identf_Movd, Cd_Lote&#x29;&#xd;&#xa;Values &#x28;T.Dt_Ocorrencia,  T.Cd_Tp_Identf_Clie,  T.Cd_Doc_Identf_Clie,  T.Cd_Transacao,  T.Cd_Banco,   T.Cd_Ag, T.CD_AG_OPER,  T.Cd_Cta,  T.Cd_Trans_Orig,  T.Cd_Natur_Oper,&#xd;&#xa; T.Cd_Tp_Oper,  T.Cd_Via_Oper,  T.Cd_Forma_Oper,  T.Vl_Oper,  T.DT_TRANS,  T.Cd_Bco_Destorig,   T.Cd_Ag_Destorig,   T.Cd_Cta_Destorig,  T.Nm_Destorig, &#xd;&#xa; T.Cd_Tp_Identf_Destorig, T.Cd_Doc_Identf_Destorig,  T.Nm_Bnefc_Cartao,   T.Cd_Tp_Identf_Bnefc,  T.Cd_Doc_Identf_Bnefc,  T.Nm_Pssoa_Realz_Trans,&#xd;&#xa; T.Cd_Tp_Doc_Identf_Realz_Trans,  T.Cd_Doc_Identf_Realz_Trans,  T.Cd_Tp_Identf_Movd,  T.Nm_Movd,  T.cd_doc_identf_movd,  T.Cd_Lote&#x29;&#x3b;&#x27;&#xd;&#xa; as TRANS_COAF&#xd;&#xa;from dual</sql>
