    <sql>Select &#x27;Merge Into Tb_Clie_Coaf C  &#xd;&#xa; Using &#x28;&#xd;&#xa;  Select Dt_Ocorrencia, Cd_Doc_Identf_Clie, Cd_Tp_Identf_Clie, Max&#x28;Nm_Clie&#x29; nm_clie, Sum&#x28;Vl_Credito&#x29; vl_credito, Sum&#x28;Vl_Debito&#x29; vl_debito, Sum&#x28;Vl_Provsnmento&#x29; vl_provsnmento, Sum&#x28;Vl_Total_Movto&#x29; vl_total_movto, Max&#x28;Fl_Agente_Publico&#x29; fl_agente_publico, Max&#x28;Cd_Lote&#x29; cd_lote, MAX&#x28;FL_PPE&#x29; FL_PPE&#xd;&#xa;  From &#x28; &#xd;&#xa;    Select Trunc&#x28;T.Dt_Trans&#x29; Dt_Ocorrencia,&#xd;&#xa;      Cd_Doc_Identf_Clie,&#xd;&#xa;      cd_Tp_Identf_Clie,&#xd;&#xa;      Max&#x28;Nm_Clie&#x29; Nm_Clie,&#xd;&#xa;      Case When Cd_Natur_Oper &#x3d; 1  Then Sum&#x28;Vl_Oper&#x29; Else 0 End Vl_Credito,&#xd;&#xa;      Case When Cd_Natur_Oper &#x3d; 2  Then Sum&#x28;Vl_Oper&#x29; Else 0 End Vl_Debito, &#xd;&#xa;      Case When CD_Natur_oper &#x3d; 3 and Cd_Tp_Oper    &#x3d; 17 Then Sum&#x28;Vl_Oper&#x29; Else 0 End Vl_Provsnmento, &#xd;&#xa;      Sum&#x28;Vl_Oper&#x29; Vl_Total_Movto,&#xd;&#xa;      Max&#x28;Fl_Agente_Publico&#x29; Fl_Agente_Publico, &#xd;&#xa;      Cd_Lote,&#xd;&#xa;	  FL_PPE&#xd;&#xa;      From Tb_Trans T, &#x28;select trunc&#x28;min&#x28;dt_trans&#x29;&#x29; dt_min, trunc&#x28;max&#x28;dt_trans&#x29;&#x29; dt_max from tb_trans Where Cd_Lote &#x3d; &#x27; &#x7c;&#x7c; &#x24;&#x7b;LOTE&#x7d; &#x7c;&#x7c; &#x27;&#x29; l&#xd;&#xa;      Where trunc&#x28;dt_trans&#x29; &#x3e;&#x3d; l.dt_min and trunc&#x28;dt_trans&#x29; &#x3c;&#x3d; l.dt_max    &#xd;&#xa;   AND &#x27; &#x7c;&#x7c;&#xd;&#xa;   &#x28;Select Tx_Param From Tb_Param_Global Where Nm_Campo_Param &#x3d; &#x27;pmRestricaoClie&#x27;&#x29; &#xd;&#xa;   &#x7c;&#x7c;&#xd;&#xa;  &#x27;   Group By Dt_Trans, Cd_Doc_Identf_Clie, Cd_Tp_Identf_Clie, Cd_Natur_Oper, Cd_Tp_Oper, Cd_Lote, FL_PPE&#xd;&#xa;   &#x29; s&#xd;&#xa;   Where &#x28;Vl_Credito     &#x3e;&#x3d; &#x28;Select Vl_Param_Real From Tb_Param_Global Where Nm_Campo_Param &#x3d; &#x27;&#x27;pmValorEspecie&#x27;&#x27;&#x29; &#xd;&#xa;      Or  Vl_Debito      &#x3e;&#x3d; &#x28;Select Vl_Param_Real From Tb_Param_Global Where Nm_Campo_Param &#x3d; &#x27;&#x27;pmValorEspecie&#x27;&#x27;&#x29; &#xd;&#xa;      Or  Vl_Provsnmento &#x3e;&#x3d; &#x28;Select Vl_Param_Real From Tb_Param_Global Where Nm_Campo_Param &#x3d; &#x27;&#x27;pmValorEspecie&#x27;&#x27;&#x29; &#xd;&#xa;      &#x29;  &#xd;&#xa;  group by dt_ocorrencia, cd_doc_identf_clie, cd_tp_identf_clie&#xd;&#xa;  &#x29; T &#xd;&#xa;   On &#x28;C.Dt_Ocorrencia      &#x3d; T.Dt_Ocorrencia      And  &#xd;&#xa;       C.Cd_Doc_Identf_Clie &#x3d; T.Cd_Doc_Identf_Clie And  &#xd;&#xa;       C.Cd_Tp_Identf_Clie  &#x3d; T.Cd_Tp_Identf_Clie&#x29;  &#xd;&#xa;   When Matched Then  &#xd;&#xa;     Update Set C.Vl_Credito     &#x3d; T.Vl_Credito,  &#xd;&#xa;                C.Vl_Debito      &#x3d; T.Vl_Debito,  &#xd;&#xa;                C.Vl_Provsnmento &#x3d; T.Vl_Provsnmento,  &#xd;&#xa;                C.Vl_Total_Movto &#x3d; T.Vl_Total_Movto &#xd;&#xa;   When Not Matched Then  &#xd;&#xa;     Insert &#x28;Dt_Ocorrencia, Cd_Doc_Identf_Clie, Cd_Tp_Identf_Clie, Nm_Clie, Vl_Credito, Vl_Debito, Vl_Provsnmento, Vl_Total_Movto, Fl_Agente_Publico, Cd_Lote, FL_PPE&#x29;      &#xd;&#xa;     Values &#x28;T.Dt_Ocorrencia, T.Cd_Doc_Identf_Clie, T.Cd_Tp_Identf_Clie, T.Nm_Clie, T.Vl_Credito, T.Vl_Debito, T.Vl_Provsnmento, T.Vl_Total_Movto, T.Fl_Agente_Publico, T.Cd_Lote, T.FL_PPE&#x29;&#x3b; &#x27;&#xd;&#xa;     &#xd;&#xa; AS CLIE_COAF   &#xd;&#xa;FROM Dual</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
