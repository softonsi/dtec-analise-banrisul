    <sql>TRUNCATE TABLE TB_ARVORE_SOCTRIA&#x3b;&#xd;&#xa;INSERT INTO TB_ARVORE_SOCTRIA &#x28;cd_doc_identf_sociedd,CD_TP_IDENTF_SOCIEDD,CD_DOC_IDENTF_SOCIO,CD_TP_IDENTF_SOCIO,PC_PARTC_SOCIO,CD_PAIS_SOCIO,NM_SOCIEDD,CD_NIVEL,NM_CAMINHO_ARVORE&#x29;&#xd;&#xa;SELECT distinct CONNECT_BY_ROOT &#xd;&#xa;cd_doc_identf_sociedd, cd_tp_identf_sociedd , cd_doc_identf_socio , cd_tp_identf_socio ,  PC_PARTC_SOCIO, cd_pais_socio, nm_sociedd,&#xd;&#xa;LEVEL-1 cd_nivel, SYS_CONNECT_BY_PATH&#x28;&#x27;CPF&#x7c;CNPJ&#x3a; &#x27; &#x7c;&#x7c; cd_doc_identf_socio &#x7c;&#x7c; &#x27;&#x3b;&#x27; &#x7c;&#x7c; &#x27;Nome&#x3a; &#x27;  &#x7c;&#x7c; nm_socio &#x7c;&#x7c; &#x27;&#x3b;&#x27; &#x7c;&#x7c; &#x27;Participa&#xe7;&#xe3;o&#x3a; &#x27; &#x7c;&#x7c; &#x28;select NM_TP_PARTC_SOCIO FROM TB_TP_PARTIC_SOCIO where CD_TP_PARTC_SOCIO &#x3d; cs.CD_TP_PARTC_SOCIO&#x29; &#x7c;&#x7c; &#x27;&#x3b;&#x27; &#x7c;&#x7c; &#x27;&#x25; de Participa&#xe7;&#xe3;o&#x3a; &#x27; &#x7c;&#x7c; PC_PARTC_SOCIO &#x7c;&#x7c; &#x27;&#x3b;&#x27; &#x7c;&#x7c; &#x27;Pa&#xed;s&#x3a; &#x27; &#x7c;&#x7c; &#x28;select NM_PAIS FROM TB_PAIS where CD_PAIS &#x3d; cs.cd_pais_socio&#x29; &#x7c;&#x7c; &#x27;&#x3b;&#x27; &#x7c;&#x7c; &#x27;Movimentador&#x3a; &#x27; &#x7c;&#x7c; &#x28;CASE CD_IDTFD_MOVTO WHEN 1 THEN &#x27;Sim&#x27;  WHEN 0 THEN &#x27;N&#xe3;o&#x27; END&#x29;, &#x27;&#x2f;&#x27;&#x29; NM_CAMINHO_ARVORE&#xd;&#xa;FROM &#x28;SELECT CD_DOC_IDENTF_SOCIEDD,CD_TP_IDENTF_SOCIEDD, CD_DOC_IDENTF_SOCIO, CD_TP_IDENTF_SOCIO, CD_TP_PARTC_SOCIO, NM_SOCIEDD, NM_SOCIO, CD_IDTFD_MOVTO, NM_TP_PARTC_SOCIO, CD_IDENTF_REF_SOCIEDD, CD_IDENTF_REF_SOCIO, PC_PARTC_SOCIO, CD_PAIS_SOCIO, &#xd;&#xa;LEVEL FROM tb_cadeia_soctria CONNECT BY PRIOR cd_doc_identf_socio &#x3d; cd_doc_identf_sociedd&#x29; cs&#xd;&#xa;WHERE LEVEL &#x3e;&#x3d; 0 &#xd;&#xa;CONNECT BY PRIOR cd_doc_identf_socio &#x3d; cd_doc_identf_sociedd&#x3b;&#xd;&#xa;&#xd;&#xa;TRUNCATE TABLE TB_BNEFC_FINAL_SOCTRIA&#x3b;&#xd;&#xa;INSERT INTO TB_BNEFC_FINAL_SOCTRIA &#x28;CD_DOC_IDENTF_SOCIEDD,CD_TP_IDENTF_SOCIEDD,CD_SEQ_SOCIEDD,NM_SOCIEDD,CD_DOC_IDENTF_FINAL,CD_TP_IDENTF_FINAL,PC_PARTC_FINAL,CD_PAIS_FINAL,NM_CAMINHO_ARVORE&#x29;&#xd;&#xa;select distinct &#xd;&#xa;CD_DOC_IDENTF_SOCIEDD , cd_tp_identf_sociedd, row_number&#x28;&#x29; OVER &#x28;ORDER BY CD_DOC_IDENTF_SOCIEDD&#x29; CD_SEQ_SOCIEDD, &#x28;select max&#x28;nm_sociedd&#x29; from tb_cadeia_soctria where cd_doc_identf_sociedd &#x3d; b.cd_doc_identf_sociedd&#x29; nm_sociedd, &#xd;&#xa;cd_doc_identf_socio, cd_tp_identf_socio, PC_PARTC_SOCIO, cd_pais_socio, NM_CAMINHO_ARVORE&#xd;&#xa;from tb_arvore_soctria b&#xd;&#xa;where not  exists&#xd;&#xa;&#x28;select 1 from tb_arvore_soctria c&#xd;&#xa; where   c.NM_CAMINHO_ARVORE like &#x27;&#x25;&#x27; &#x7c;&#x7c; b.NM_CAMINHO_ARVORE &#x7c;&#x7c; &#x27;&#x25;&#x27; and b.NM_CAMINHO_ARVORE &#x3c;&#x3e; c.NM_CAMINHO_ARVORE&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;TRUNCATE TABLE TB_ADMINISTRADOR_FINAL&#x3b;&#xd;&#xa;INSERT INTO TB_ADMINISTRADOR_FINAL &#x28;CD_DOC_IDENTF_ADM, CD_TP_IDENTF_ADM,CD_SEQ_ADM, NM_ADM,CD_PAIS_ADM, NM_CAMINHO_ARVORE&#x29;   &#xd;&#xa;SELECT DISTINCT CD_DOC_IDENTF_ADM, CD_TP_IDENTF_ADM, 1 CD_SEQ_ADM, NM_ADM, CD_PAIS_ADM, &#xd;&#xa;       LISTAGG&#x28;&#x27;CNPJ&#x3a; &#x27; &#x7c;&#x7c; CD_DOC_IDENTF_CLIE &#x7c;&#x7c; &#x27;&#x3b;&#x27; &#x7c;&#x7c; &#x27;Empresa&#x3a; &#x27; &#x7c;&#x7c; NM_CLIE  &#x7c;&#x7c; &#x27;&#x3b;&#x27; &#x7c;&#x7c;  &#x27;Cargo do Administrador na empresa&#x3a; &#x27; &#x7c;&#x7c; &#x28;select nm_cargo_adm from tb_cargo_administrador where cd_cargo_adm &#x3d; a.cd_cargo_adm&#x29;, &#x27;&#x2f;&#x27;&#x29; WITHIN GROUP &#x28;ORDER BY CD_DOC_IDENTF_ADM&#x29;&#xd;&#xa;         OVER &#x28;PARTITION BY CD_DOC_IDENTF_ADM&#x29; as &#x22;Clientes&#x22;&#xd;&#xa;FROM TB_CADEIA_ADMINISTRADOR a&#x3b;</sql>
